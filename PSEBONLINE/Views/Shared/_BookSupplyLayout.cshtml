<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title Punjab School Education Board</title>
    <link href='https://fonts.googleapis.com/css?family=Source+Sans+Pro:400,400italic,600' rel='stylesheet' type='text/css'>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>
    <link rel="stylesheet" href="https://code.jquery.com/ui/1.12.0/themes/base/jquery-ui.css">
            <noscript>
            <style type="text/css">
                #HPL {display:none}
            </style>
        <charset style="background-color:#cc0000;color:#ffd800;font-size:24px;text-align:center;padding:10px 0">Your browser does not support JavaScript!</charset>

    </noscript> 
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
        <div id="HPL">
    <div class="header">
        <div class="container">
            <div class="row">
                <div class="col-xs-3 col-md-4"><img src="~/Images/pseb_logo.png" alt="Punjab School Education Board" class="img-responsive" /></div>
                <div class="col-xs-9 col-md-8"><img src="~/Images/PSEB_Text.png" alt="Punjab School Education Board" class="img-responsive" /></div>
                <div class="clearfix"></div>
                <div class="col-md-12 text-center" style="color:#fff;font-size:24px">
                    &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; &nbsp;&nbsp;&nbsp;Printer Name :
                    @if (Session["PrinterName"] != null)
                    {@Session["PrinterName"].ToString()}
                else { Response.Redirect("~/BookSupply/Login"); }
                </div>
            </div></div></div>
    <div class="navbar navbar-inverse">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @*@Html.ActionLink("Index", "Home", new { area = "" }, new { @class = "navbar-brand" })*@
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Home", "Welcome", "BookSupply")</li>
                    @*<li>@Html.ActionLink("Profile", "ViewProfile", "Printer")</li>*@ 
                </ul>
                <ul class="nav navbar-nav navbar-right">
                    <li>@Html.ActionLink("Logout", "Logout", "BookSupply")</li>
                </ul>
            </div>
        </div>
    </div>

    <div class="container KDBG">
        @RenderBody()
    </div>

            <footer class="bs-docs-footer">
                <div class="container text-center">
                    All Rights Reserved at Punjab School Education Board, 1969 - 2016
                </div>
            </footer>
            </div>  
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>