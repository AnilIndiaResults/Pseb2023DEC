<div class="row">
    <div class="col-md-12">
        <div class="table-responsive">
            <table class="table table-bordered" id="t01">
                <tr class="active-tr">
                    <th>Std ID</th>
                    <th>Form</th>
                    <th>Reg.No.</th>
                    <th>Name/Father's Name/Mother's Name</th>
                    <th>RegDate</th>
                    <th>Result Status</th>
                    <th>Import Status</th>
                    <th>Lot</th>
                    <th>TC Ref No</th>
                    <th>ACTIONS</th>
                </tr>
                @{
                    if (ViewBag.TotalCount > 0)
                    {
                        for (int i = 0; i < Model.StoreAllData.Tables[0].Rows.Count; i++)
                        {
                            @Html.HiddenFor(a => a.SelYear) ;
                            var year = Model.SelYear.ToString();
                            var idno = Model.StoreAllData.Tables[0].Rows[i]["Std_id"].ToString();
                            var Sno = Model.StoreAllData.Tables[0].Rows[i]["SRL"].ToString();
                            var RegNo = Model.StoreAllData.Tables[0].Rows[i]["regno"].ToString();
                            var FormName = Model.StoreAllData.Tables[0].Rows[i]["FORM"].ToString();
                            var Candi_Name = Model.StoreAllData.Tables[0].Rows[i]["NAME"].ToString();
                            var Father_Name = Model.StoreAllData.Tables[0].Rows[i]["FNAME"].ToString();
                            var Mother_Name = Model.StoreAllData.Tables[0].Rows[i]["MNAME"].ToString();
                            var FullName = Candi_Name + "/ " + Father_Name + "/ " + Mother_Name;
                            var DOB = Model.StoreAllData.Tables[0].Rows[i]["DOB"].ToString();
                            var Gender = Model.StoreAllData.Tables[0].Rows[i]["Gender"].ToString();
                            var RegDate = Model.StoreAllData.Tables[0].Rows[i]["regdt"].ToString();
                            var AdmDate = Model.StoreAllData.Tables[0].Rows[i]["AdmDate"].ToString();
                            var Lot = Model.StoreAllData.Tables[0].Rows[i]["LOT"].ToString();
                            var TCrefno = Model.StoreAllData.Tables[0].Rows[i]["TCrefno"].ToString();
                            var schl = Model.StoreAllData.Tables[0].Rows[i]["schl"].ToString();
                            var Tcstatus = Model.StoreAllData.Tables[0].Rows[i]["Tcstatus"].ToString();
                            var importflag = Model.StoreAllData.Tables[0].Rows[i]["importflag"].ToString();
                            var reclock = Model.StoreAllData.Tables[0].Rows[i]["reclock"].ToString();
                            var RegNoSatus = Model.StoreAllData.Tables[0].Rows[i]["RegNoSatus"].ToString();


                            if (reclock == "1")
                            {
                                reclock = "Submitted";
                            }
                            else
                            {
                                reclock = "Pending";
                            }
                            if (importflag == "1")
                            {
                                importflag = "Imported";//schl;
                            }
                            else
                            {
                                importflag = "Pending";
                            }

                            <tr>
                                <td>
                                    @idno
                                </td>
                                <td>
                                    @FormName
                                </td>
                                <td>
                                    @RegNo
                                </td>
                                <td>
                                    @FullName
                                </td>
                                <td>
                                    @RegDate
                                </td>
                                <td>
                                    @reclock
                                </td>
                                <td>
                                    @importflag
                                </td>
                                <td>
                                    @Lot
                                </td>
                                <td>
                                    @TCrefno
                                </td>
                                <td>
                                    <div>
                                        @{

                                            if (RegNoSatus.ToUpper() == "Alloted".ToUpper())
                                            {

                                                if (@Tcstatus == "2" && @TCrefno != "")
                                                {
                                                    @Html.ActionLink("Print", "TCPrintSchl", new { id = encrypt.QueryStringModule.Encrypt(idno), year = encrypt.QueryStringModule.Encrypt(year) }, new { Class = "", title = "TCPrint" })
                                                }
                                                else if (@importflag != "Pending")
                                                {
                                                    <span>Imported</span>
                                                }
                                                else if (@Tcstatus == "2" && @TCrefno != "" && @importflag != "Pending")
                                                {
                                                    @*<span> Done</span>
                                                        <span> | </span>*@
                                                    @Html.ActionLink("Print", "TCPrintSchl", new { id = encrypt.QueryStringModule.Encrypt(idno), year = encrypt.QueryStringModule.Encrypt(year) }, new { Class = "", title = "TCPrint" })
                                                    @*<span> | </span>
                                                        @Html.ActionLink("Cancel", "TCCancelSchl", new { id = idno }, new { onclick = "return confirm('Are you sure want to Cancel TC ?');" })*@
                                                }
                                                else
                                                {
                                                    if (@reclock == "Pending")
                                                    {
                                                        @Html.ActionLink("TC", "TCRequestSchl", new { id = encrypt.QueryStringModule.Encrypt(idno), year = encrypt.QueryStringModule.Encrypt(year) }, new { Class = "", title = "TC can not Generate Due to Result not Update", onclick = "return confirm('TC Can not Generate, Due to Result Not Updated/Not Final Submited/Already Imported');" })
                                                    }
                                                    else if (@reclock == "Submitted" && @importflag == "Pending")
                                                    {
                                                        //@Html.ActionLink("TC", "TCRequestDoneSchl", new { id = encrypt.QueryStringModule.Encrypt(idno), year = encrypt.QueryStringModule.Encrypt(year) }, new { Class = "", title = "Click for TC Generate", onclick = "return tcDateVal()" })
                                                        @*@Html.ActionLink("TC", "TCRequestSchl", new { id = encrypt.QueryStringModule.Encrypt(idno), year = encrypt.QueryStringModule.Encrypt(year) }, new { Class = "", title = "TC can not Generate Due to Last Date was 31-Aug-2018", onclick = "return confirm('TC Can not Generate, Due to Last Date was 31-Aug-2018');" })*@
                                                        @Html.ActionLink("TC", "TCRequestDoneSchl", new { id = encrypt.QueryStringModule.Encrypt(idno), year = encrypt.QueryStringModule.Encrypt(year) }, new { Class = "", title = "Click for TC Generate", onclick = "return tcDateVal();" })
                                                    }
                                                    else
                                                    {
                                                        @Html.ActionLink("TC", "TCRequestDoneSchl", new { id = encrypt.QueryStringModule.Encrypt(idno), year = encrypt.QueryStringModule.Encrypt(year) }, new { Class = "", title = "Click for TC Generate", onclick = "return tcDateVal();" })
                                                    }
                                                }
                                            }
                                        }
                                    </div>
                                </td>
                            </tr>
                        }
                    }
                    else
                    {
                        <tr>
                            <td colspan="8">
                                @ViewBag.Message
                            </td>
                        </tr>
                    }
                }
            </table>
        </div>
    </div>
</div>

