            width: 450,
            height: 250,
            buttons: {
                Close: function () {
                    $(this).dialog("close");
                }
            }
        });
    });
</script>
<div class="modal fade" id="myModal" role="dialog">

    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <div class="modal-title">Forget Password</div>
            </div>
            <div class="modal-body">
                <center> <input type="text" id="txtAppno" onkeypress="return isNumberKey(event);" placeholder="Enter Application No." name="txtAppno" maxlength="15" required /></center>
                <center> <input type="text" id="txtAadhar" onkeypress="return isNumberKey(event);" placeholder="Enter Aadhar No." name="txtAadhar" maxlength="12" required /></center>
                <center> <input type="text" id="txtDOB" onkeydown="return false;" placeholder="Select Date of Birth" name="txtDOB" maxlength="10" class="date" required oncopy="return false;" oncut="return false;" onpaste="return false;" /></center>

                <input type="button" class="btn" value="Send" id="btnSend" /> <div id="loading1"></div>

            </div>
            <div class="modal-footer">
                <button type="button" class="btn" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>

<script type="text/javascript">

    $(function () {
        $(".date").datepicker({
            dateFormat: "dd/mm/yy",
            changeMonth: true,
            changeYear: true,
            defaultDate: new Date('1 January 1990'),
            yearRange: '1900:' + (new Date().getFullYear() - 13)
        });
    });

    function isNumberKey(evt) {
        var charCode = (evt.which) ? evt.which : event.keyCode
        if ((charCode >= 48 && charCode <= 57) || (charCode == 8) || (charCode == 46)) {
            return true;
        }
        else {
            alert("Invalid character...Enter Only Number!!");
            return false;
        }
    }
</script>



<script type="text/javascript">
    $(document).ready(function () {   //Start Compose Password
        $("#btnSend").click(function () {
            // alert("Start Forget Password ");
            if ($("#txtAppno").val() == "") {
                $("#txtAppno").addClass("input-validation-error");
                return false;
            }
            if ($("#txtAadhar").val() == "") {
                $("#txtAadhar").addClass("input-validation-error");
                return false;
            }
            if ($("#txtDOB").val() == "") {
                $("#txtDOB").addClass("input-validation-error");
                return false;
            }
            else {
                //  alert('Now Send Password to :' + $("#txtAppno").val())
                // $('#Main1').hide();
                $('#loading1').html("<p><img src='https://registration2022.pseb.ac.in/images/loadingk.gif'></p>");
                $.ajax({
                    type: "POST",
                    url: "/Open/JqForgotPassword",
                    data: '{storeid:"' + $("#txtAppno").val() + '",aadhar:"' + $("#txtAadhar").val() + '" ,dob:"' + $("#txtDOB").val() + '"  }',
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    success: OnSuccess,
                    failure: function (response) {
                        alert(response);
                        $('#loading1').empty();
                    }
                });
                function OnSuccess(response) {
                    if (response.dee == "1") {
                        var result = 'Your Password of Application Number ' + $("#txtAppno").val() + ' is : ' + response.result;
                        alert(result);
                        window.location.href = '@Url.Action("Index", "Open")';
                            $("#myModal").hide();
                            $('#loading1').empty();
                        }
                        else {
                            alert("Details Not Matched, Please Try Again with Correct Details");
                            window.location.href = '@Url.Action("Index", "Open")';
                            $("#myModal").hide();
                            $('#loading1').empty();
                        }
                    }
                }
            });

            //End  Compose SMS
        });




</script>