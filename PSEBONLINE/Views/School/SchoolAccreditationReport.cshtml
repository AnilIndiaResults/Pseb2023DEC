@model PSEBONLINE.Models.SchoolAccreditationCombinedModel
@{
    ViewBag.Title = "School Accreditation Report";
    Layout = "~/Views/Shared/_Print_Header.cshtml";
}

<link rel="stylesheet" href="/Content/cardprinta.css" />
<style type="text/css">
    .tprint td {
        padding: 1px 2px;
        line-height: 16px;
    }
</style>
<table style="width:100%;" border="0" class="bgimg">
    <tr>
        <td>
            <table style="width:100%;" border="0" class="noprint">
                <tr>
                    <td style="text-align:right;padding-bottom:3px">
                        <input type="button" value="Print" class="btn" onclick="window.print()" />
                        <input type="button" value="Back" class="btn" onclick="history.go(-1);" />
                    </td>
                </tr>
            </table>
        </td>
    </tr>
    @if (Model != null && ViewBag.TotalCount > 0)
    {
        if (Model.schlmodel != null && Model.sam != null)
        {
            <tr>
                <td align="center">
                    <table border="0" style="width:98%;" align="center" class="brack">
                        <thead>
                            <tr>
                                <td colspan="2" style="padding-bottom:5px">
                                    <table width="100%">
                                        <tr>
                                            <td width="10%"><img src="~/Images/Printlogo.jpg" alt="PSEB" /></td>
                                            <td width="60%" style="font-size:16px;text-align:center;line-height:25px;">
                                                <span style="font-size:26px;">Punjab School Education Board</span><br />Accreditation/Renewal Form for Open School Study Centre, Session 2023-24
                                            </td>
                                            <td width="30%" align="right">
                                                DATE :@DateTime.Now.ToString("dd/MM/yyyy")<br />
                                            </td>
                                        </tr>
                                    </table>
                                </td>

                            </tr>
                            <tr>
                                <td colspan="3">                                   
                                    Ref. No:- @Model.sam.StoreAllData.Tables[0].Rows[0]["refno"].ToString()  School Code:-  @Session["SCHL"].ToString()  Study Centre Code :-
                                   
                                </td>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td colspan="3">
                                    <div class="row">
                                        <div class="col-md-2">
                                            School Code <span class="rqf">*</span>
                                        </div>
                                        <div class="col-md-2">
                                            @Html.DisplayFor(a => a.schlmodel.SCHL, new { })
                                        </div>
                                        <div class="col-md-2">
                                            School Identity Code <span class="rqf">*</span>
                                        </div>
                                        <div class="col-md-2">@Html.DisplayFor(a => a.schlmodel.idno, new { })</div>
                                        <div class="col-md-2">
                                            Class Level <span class="rqf">*</span>
                                        </div>
                                        <div class="col-md-2">@Html.DisplayFor(a => a.schlmodel.CLASS, new { })</div>

                                        <div class="col-md-2">School Name(English) <span class="rqf">*</span></div>
                                        <div class="col-md-10">
                                            @Html.DisplayFor(a => a.schlmodel.SCHLE, new { })
                                        </div>

                                        <div class="col-md-2">
                                            District Name<span class="rqf">*</span>
                                        </div>
                                        <div class="col-md-2">@Html.DisplayFor(a => a.schlmodel.DISTNM, new { })</div>
                                        <div class="col-md-2">
                                            Establishment Year <span class="rqf">*</span>
                                        </div>
                                        <div class="col-md-2">@Html.DisplayFor(a => a.schlmodel.SchlEstd, new { })</div>
                                        <div class="col-md-2">
                                            Type of School  <span class="rqf">*</span>
                                        </div>
                                        <div class="col-md-2">@Html.DisplayFor(a => a.schlmodel.USERTYPE, new { })</div>

                                        <div class="col-md-2">
                                            Address of School  <span class="rqf">*</span>
                                        </div>
                                        <div class="col-md-8">@Html.DisplayFor(a => a.schlmodel.ADDRESSE, new { })</div>

                                    </div>
                                    <div class="clearfix"></div>

                                    <h4>Class/Stream Details</h4>
                                    <div class="row">
                                        <div class="col-md-12">
                                            <table id="t01" class="table table-bordered">
                                                <tr style="font-weight: bold;">
                                                    <td>Class/Group</td>
                                                    <td align="left">
                                                        MATRIC
                                                    </td>
                                                    <td align="left">
                                                        Humanities
                                                    </td>
                                                    <td align="left">
                                                        Science
                                                    </td>
                                                    <td align="left">
                                                        Commerce
                                                    </td>
                                                    <td align="left">
                                                        Vocational
                                                    </td>
                                                </tr>
                                                <tr style="font-weight: bold;">
                                                    <td>Affiliation Type</td>
                                                    <td align="left">
                                                        @Html.DisplayFor(a => a.schlmodel.HID_UTYPE, new { })
                                                    </td>
                                                    <td align="left">
                                                        @Html.DisplayFor(a => a.schlmodel.H_UTYPE, new { })
                                                    </td>
                                                    <td align="left">
                                                        @Html.DisplayFor(a => a.schlmodel.S_UTYPE, new { })
                                                    </td>
                                                    <td align="left">
                                                        @Html.DisplayFor(a => a.schlmodel.C_UTYPE, new { })
                                                    </td>
                                                    <td align="left">
                                                        @Html.DisplayFor(a => a.schlmodel.V_UTYPE, new { })
                                                    </td>
                                                </tr>
                                                <tr style="font-weight: bold;">
                                                    <td>East. Year</td>
                                                    <td>@Html.DisplayFor(a => a.schlmodel.HID_YR, new { })</td>
                                                    <td>@Html.DisplayFor(a => a.schlmodel.HYR, new { })</td>
                                                    <td>@Html.DisplayFor(a => a.schlmodel.SYR, new { })</td>
                                                    <td>@Html.DisplayFor(a => a.schlmodel.CYR, new { })</td>
                                                    <td>@Html.DisplayFor(a => a.schlmodel.VYR, new { })</td>
                                                </tr>
                                                @*<tr style="font-weight: bold;">
            <td>Section</td>
            <td>@Html.DisplayFor(a => a.schlmodel.HID_YR_SEC, new { })</td>
            <td>@Html.DisplayFor(a => a.schlmodel.HYR_SEC, new { })</td>
            <td>@Html.DisplayFor(a => a.schlmodel.SYR_SEC, new { })</td>
            <td>@Html.DisplayFor(a => a.schlmodel.CYR_SEC, new { })</td>
            <td>@Html.DisplayFor(a => a.schlmodel.VYR_SEC, new { })</td>
        </tr>*@
                                                <tr style="font-weight: bold;">
                                                    <td>Last Session Open Accreditation</td>
                                                    <td>@Html.DisplayFor(a => a.schlmodel.OMATRIC, new { })</td>
                                                    <td>@Html.DisplayFor(a => a.schlmodel.OHUM, new { })</td>
                                                    <td>@Html.DisplayFor(a => a.schlmodel.OSCI, new { })</td>
                                                    <td>@Html.DisplayFor(a => a.schlmodel.OCOMM, new { })</td>
                                                    <td>@Html.DisplayFor(a => a.schlmodel.OVOC, new { })</td>
                                                </tr>

                                            </table>

                                        </div>


                                    </div>
                                    <div class="clearfix"></div>





                                    <h4>Principal / Contact Details </h4>
                                    <div class="row">
                                        <div class="col-md-2">
                                            Principal Name <span class="rqf">*</span>
                                        </div>

                                        <div class="col-md-2">
                                            @Html.DisplayFor(a => a.schlmodel.PRINCIPAL, new { id = "txtPrincipal",  })
                                        </div>
                                        <div class="col-md-2">
                                            Office Phone No. <span class="rqf">*</span>
                                        </div>
                                        <div class="col-md-2">
                                            @Html.DisplayFor(a => a.schlmodel.STDCODE, new { id = "txtstdCode" }) @Html.DisplayFor(a => a.schlmodel.PHONE, new { id = "txtPhone" })
                                        </div>
                                        <div class="col-md-2">
                                            Mobile No. <span class="rqf">*</span>
                                        </div>
                                        <div class="col-md-2">
                                            @Html.DisplayFor(a => a.schlmodel.MOBILE, new { id = "txtMob",  })
                                        </div>
                                        <div class="clearfix"></div>
                                        <div class="col-md-2">
                                            Registered Email <span class="rqf">*</span>
                                        </div>
                                        <div class="col-md-2">
                                            @Html.DisplayFor(a => a.schlmodel.EMAILID, new {  })
                                        </div>
                                       
                                        <div class="col-md-2">Other Person Name <span class="rqf">*</span></div>
                                        <div class="col-md-2">
                                            @Html.DisplayFor(a => a.schlmodel.CONTACTPER, new { id = "txtOtherContactper",  })
                                        </div>

                                        <div class="col-md-2">
                                            Other Mobile No. <span class="rqf">*</span>
                                        </div>
                                        <div class="col-md-2">
                                            @Html.DisplayFor(a => a.schlmodel.OtContactno, new { id = "txtOtherMob",  })
                                        </div>
                                    </div>

                                    <h4>School Bank Account Details  (Used for credit payment issued by Board)</h4>
                                    <div class="row">
                                        <div class="col-md-12">
                                            <div class="row">
                                                <div class="col-md-3">
                                                    Name of Bank<span class="rqf">*</span>
                                                    @Html.DisplayFor(a => a.schlmodel.Bank, new { id = "Bank" })
                                                </div>
                                                <div class="col-md-2">
                                                    Bank IFSC Code<span class="rqf">*</span>
                                                    @Html.DisplayFor(a => a.schlmodel.IFSC, new { id = "IFSC" })
                                                </div>
                                                <div class="col-md-2">
                                                    Enter A/c No <span class="rqf">*</span>
                                                    @Html.DisplayFor(a => a.schlmodel.acno, new { id = "acno" })
                                                </div>
                                                <div class="col-md-2">
                                                    Confirm A/c No  <span class="rqf">*</span>
                                                    @Html.DisplayFor(a => a.schlmodel.confirmacno, new { id = "confirmacno" })
                                                </div>
                                                <div class="clearfix"></div>

                                            </div>
                                        </div>
                                    </div>




                                </td>
                            </tr>

                            <tr>
                                <td colspan="3">
                                    <table id="" class="table table-bordered">
                                        <tr class="active-tr">
                                            <th>Sr No</th>
                                            <th>Class</th>
                                            <th>Group</th>
                                            <th>Accreditation</th>
                                        </tr>
                                        @{
                                            if (Model.sam.StoreAllData.Tables[0].Rows.Count > 0)
                                            {
                                                int d = 0;
                                                int Count = 0;
                                                for (int i = 0; i < Model.sam.StoreAllData.Tables[0].Rows.Count; i++)
                                                {
                                                    d = d + 1;
                                                    var ID = Model.sam.StoreAllData.Tables[0].Rows[i]["ID"].ToString();
                                                    var Class = Model.sam.StoreAllData.Tables[0].Rows[i]["Class"].ToString();
                                                    var Group = Model.sam.StoreAllData.Tables[0].Rows[i]["exam"].ToString();
                                                    var Accreditation = Model.sam.StoreAllData.Tables[0].Rows[i]["Acrtype"].ToString();
                                                    var refno = Model.sam.StoreAllData.Tables[0].Rows[i]["refno"].ToString();
                                                    var Fee = Model.sam.StoreAllData.Tables[0].Rows[i]["Fee"].ToString();
                                                    var latefee = Model.sam.StoreAllData.Tables[0].Rows[i]["latefee"].ToString();
                                                    var FeeStatus = Model.sam.StoreAllData.Tables[0].Rows[i]["FeeStatus"].ToString();
                                                    var challanid = Model.sam.StoreAllData.Tables[0].Rows[i]["challanid"].ToString();
                                                    var Challanverify = Model.sam.StoreAllData.Tables[0].Rows[i]["Challanverify"].ToString();
                                                    var IsFinal = Model.sam.StoreAllData.Tables[0].Rows[i]["IsFinal"].ToString();
                                                    Count = Count + 1;
                                                    <tr>
                                                        <td>@Count</td>
                                                        <td>@Class</td>
                                                        <td>@Group</td>
                                                        <td>@Accreditation</td>
                                                    </tr>
                                                }

                                            }

                                        }
                                    </table>

                                </td>
                            </tr>

                            <tr>
                                <td colspan="3">
                                    <p style="text-align:center;font-size:18px;color:#f00">Fee & Challan Details :-</p>
                                    @{

                                        if (Model.sam.StoreAllData.Tables[1].Rows.Count > 0)
                                        {
                                            for (int i = 0; i < Model.sam.StoreAllData.Tables[1].Rows.Count; i++)
                                            {
                                                var RefNo = Model.sam.StoreAllData.Tables[1].Rows[i]["RefNo"].ToString();
                                                var GroupList = Model.sam.StoreAllData.Tables[1].Rows[i]["GroupList"].ToString();
                                                var Bank = Model.sam.StoreAllData.Tables[1].Rows[i]["Bank"].ToString();
                                                var ChallanId = Model.sam.StoreAllData.Tables[1].Rows[i]["ChallanId"].ToString();
                                                var ChallanDate = Model.sam.StoreAllData.Tables[1].Rows[i]["CHLNDATE"].ToString();
                                                var ChallanVDate = Model.sam.StoreAllData.Tables[1].Rows[i]["CHLNVDATE"].ToString();
                                                var ChallanVerifiedOn = Model.sam.StoreAllData.Tables[1].Rows[i]["ChallanVerifiedOn"].ToString();
                                                var Status = Model.sam.StoreAllData.Tables[1].Rows[i]["Status"].ToString();
                                                var FEE = Model.sam.StoreAllData.Tables[1].Rows[i]["FEE"].ToString();
                                                var LateFee = Model.sam.StoreAllData.Tables[1].Rows[i]["LateFee"].ToString();
                                                var TotalFee = Model.sam.StoreAllData.Tables[1].Rows[i]["TotalFee"].ToString();

                                                var BRANCH = Model.sam.StoreAllData.Tables[1].Rows[i]["BRANCH"].ToString();
                                                var J_REF_NO = Model.sam.StoreAllData.Tables[1].Rows[i]["J_REF_NO"].ToString();
                                                var DEPOSITDT = Model.sam.StoreAllData.Tables[1].Rows[i]["DEPOSITDT"].ToString();
                                                var VERIFYDATE = Model.sam.StoreAllData.Tables[1].Rows[i]["VERIFYDATE"].ToString();


                                                <table width="100%" border="1">
                                                    <tr>
                                                        <td>&nbsp;Challan Id</td>
                                                        <td>&nbsp;@ChallanId</td>
                                                        <td>&nbsp;Challan Generate Date</td>
                                                        <td>&nbsp;@ChallanDate</td>
                                                        <td>&nbsp;Challan Valid Date</td>
                                                        <td>&nbsp;@ChallanVDate</td>
                                                    </tr>
                                                    <tr>
                                                        <td>&nbsp;Fee</td>
                                                        <td>&nbsp;@FEE</td>
                                                        <td>&nbsp;Late Fee</td>
                                                        <td>&nbsp;@LateFee</td>
                                                        <td>&nbsp;Total Fee</td>
                                                        <td>&nbsp;@TotalFee</td>
                                                    </tr>
                                                    <tr>
                                                        <td>&nbsp;Bank & Branch Name</td>
                                                        <td colspan="5">&nbsp;@BRANCH</td>
                                                    </tr>
                                                    <tr>
                                                        <td>&nbsp;Bank Ref No</td>
                                                        <td>&nbsp;@J_REF_NO</td>
                                                        <td>&nbsp;</td>
                                                        <td>&nbsp;</td>
                                                        <td>&nbsp;Fee Deposit Date</td>
                                                        <td>&nbsp;@DEPOSITDT</td>
                                                    </tr>
                                                </table>



                                            }
                                        }
                                    }



                                </td>
                            </tr>
                        </tbody>
                    </table>

                </td>
            </tr>

        }

    }




</table>


