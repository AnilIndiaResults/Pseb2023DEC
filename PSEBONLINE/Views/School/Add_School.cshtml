            Password @Html.TextBoxFor(a => a.PASSWORD, new { id = "PASSWORD", @maxlength = "20", @class = "textnone" })
        </div>
        <div class="col-md-4">
            Validity @Html.TextBoxFor(a => a.VALIDITY, new { @Value = "9999", id = "VALIDITY", @maxlength = "10", onkeypress = "return isNumberKey(event);" })
