<h1>Add School Form</h1>

<h4>School Identity Details</h4>

@using (Html.BeginForm("Add_School", "School", FormMethod.Post, new { id = "searchform", enctype = "multipart/form-data" }))
{
    <div class="row">
        <div class="col-md-4" style="display:none">
            School Code @Html.TextBoxFor(a => a.SCHL, new { id = "SchoolCode", @maxlength = "7", onkeypress = "return isNumberKey(event);" })
        </div>
        <div class="col-md-4">
            ID No. @Html.RadioButtonFor(m => m.SameAsSchl, "Yes", new { id = "SameAsSchl" })   <span>Same As Schl</span>
            <br />
            @Html.TextBoxFor(a => a.idno, new { id = "IDNo", @maxlength = "10" })
        </div>
        <div class="col-md-4">
            Study Center Code @Html.TextBoxFor(a => a.OCODE, new { id = "OCODE", @maxlength = "7", onkeypress = "return isNumberKey(event);" })
        </div>
        <div class="col-md-4">
            Udise Code @Html.TextBoxFor(a => a.udisecode, new { id = "udisecode", onkeypress = "return isNumberKey(event);" })
        </div>
        <div class="clearfix"></div>
        <div class="col-md-4">
            Session
            @Html.DropDownList("Session", new SelectList(ViewBag.SessionList, "Value", "Text", "2019-2020"), "--Select Session---", new { id = "Session", required = "Required" })

        </div>
        <div class="col-md-4">
            School Type
            @Html.DropDownList("USERTYPE", new SelectList(ViewBag.SchoolTypeList, "Value", "Text"), "--Select School Type---", new { id = "USERTYPE" })
        </div>
        <div class="col-md-4">
            Class
            @Html.DropDownList("CLASS", new SelectList(ViewBag.ClassTypeList, "Value", "Text"), "--Select Class---", new { id = "CLASS" })
        </div>
        <div class="clearfix"></div>

        <div class="col-md-4">
            Status
            @Html.DropDownList("Status", new SelectList(ViewBag.StatusList, "Value", "Text"), new { id = "Status", @readonly = "readonly" })
        </div>
        <div class="col-md-4">
            Approved
            @Html.DropDownList("Approved", new SelectList(ViewBag.YesNoList, "Value", "Text", "Y"), new { id = "Approved" })
        </div>
        <div class="col-md-4" style="display:none">
            Verified  @Html.DropDownList("vflag", new SelectList(ViewBag.YesNoList, "Value", "Text"), new { id = "vflag" })
        </div>


