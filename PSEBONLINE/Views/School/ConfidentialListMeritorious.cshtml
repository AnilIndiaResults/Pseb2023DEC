
@model PSEBONLINE.Models.SchoolModels
@{
    ViewBag.Title = "Confidential List Meritorious";
    Layout = "~/Views/Shared/_Card_Header.cshtml";
}
<link rel="stylesheet" href="/Content/cardprinta.css" />
<style type="text/css">
    body {
        font-size: 11px;
    }

    table {
        page-break-inside: auto
    }

    tr {
        page-break-inside: avoid;
        page-break-after: auto
    }

    thead {
        display: table-header-group
    }

    tfoot {
        display: table-footer-group
    }
</style>
<style type="text/css">
    .brack {
        page-break-after: always;
    }
</style>
@using (Html.BeginForm("ConfidentialListMeritorious", "School", FormMethod.Post, new { id = "searchform" }))
