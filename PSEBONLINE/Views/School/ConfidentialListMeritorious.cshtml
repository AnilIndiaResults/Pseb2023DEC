    function valForm()
    {
        var schoolcode = document.getElementById("ExamCent").value;
        if (schoolcode == "" || document.getElementById("ExamCent").value == null) {
        alert("Select Block");
        document.getElementById("ExamCent").focus();
        return false;
    }

    }
</script> 