    function SubMarks(k, nxt, prv, schl) {

        var rl = $("#ResultList-" + k).val();
        if (rl == "" || rl == null) {
            alert("Please Select Result");
            rl.focus();
            return false;
        }
        var totMarks = $("#totMarks-" + k).val();
        var obtMarks = $("#obtainMarks-" + k).val();
        var UPTREMARKS = $("#UPTREMARKS-" + k).val();
        if (parseInt(obtMarks) > parseInt(totMarks)) {
            alert("Obtaied marks should Not be Greater Than Total Marks");
            obtMarks.focus();
            return false;
        }
        if (rl == "1" || rl == "2" || rl == "3") {

            if (totMarks == "" || totMarks == null) {
                alert("Please Fill Total marks");
                totMarks.focus();
                return false;
            }
            if (obtMarks == "" || obtMarks == null) {
                alert("Please Fill Obtained marks");
                obtMarks.focus();
                return false;
            }

        }



        var tblname = '#subjects-' + k;
        var tblnxt = '#subjects-' + nxt;

        var TableData;
        TableData = JSON.stringify(storeTblValues());

        if (TableData == "false") {
            alert("Please Fill Subject OBT-Marks, OBTMarks can be in-between Min and Max Marks");

        }
        else if (TableData != "false") {
            // alert('Now Start:' + TableData);
            $('#loading1-' + k).html("<p><img src='https://registration2022.pseb.ac.in/images/loadingk.gif'></p>");
            $.post("/School/UpdateEleventhResult", { "ResultList": rl, "totmarks": totMarks, "obtmarks": obtMarks, "stdid": k, "schl": schl, "UPTREMARKS": UPTREMARKS },

                function (data) {
                    $('#loading1-' + k).empty();

                    if (data.status == "0" || data.status == "-1") {

                        alert("Subject Marks Alloted Failure, Please Try Again");
                    }
                    else if (data.status == "-2") {

                        alert("Invalid Marks, Please Enter OBT Marks less than equal to Max Marks and Greater than Zero !!");
                    }
                    else if (data.status == "-3") {

                        alert("Please Fill All Subject OBTMARKS");
                    }
                    else {
                        alert("Subject Marks Alloted Successfully of Student : " + k);
                        nxtDiv(nxt, k, prv);
                    }
                });
        }

        function storeTblValues() {
            // alert('3');
            var TableData = new Array();
            $('' + tblname + ' tr').each(function (row, tr) {
                TableData[row] = {
                    //"SUB": $(tr).find('td:eq(0)').text()
                    "SUB": $(tr).find('.chksub').val()
                    , "OBTMARKS": $(tr).find('.chkclass').val()
                    , "MINMARKS": $(tr).find('.chkmin').val()
                    , "MAXMARKS": $(tr).find('.chkmax').val()
                }
            });
            TableData.shift();  // first row will be empty - so remove
            //alert(TableData);
            return TableData;

        }
    }

    function nxtDiv(nxt, crnt, prv) {
        // var cid = '#myModal1-' + crnt
        $('#myModal1-' + crnt).modal('hide');
        if (nxt == "0" && prv == "0") { }
        else if (nxt == "0" && prv != "0") {
            $('#myModal1-' + prv).modal('show');
        }
        else if (prv == "0" && nxt != "0") {
            $('#myModal1-' + nxt).modal('show');
        }
        else if (prv != "0" && nxt != "0") {
            $('#myModal1-' + nxt).modal('show');
        }
    }


</script>
