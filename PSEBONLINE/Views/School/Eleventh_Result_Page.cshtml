<script type="text/javascript">
    function CalculatePercentFunction(k) {
        var totMark = $("#totMarks-" + k).val();
        var obtMark = $("#obtainMarks-" + k).val();
        var Res = $("#ResultList-" + k).val();
        var comp = obtMark / totMark * 100;
        if (Res == 1) {
            if (comp < 33) {
                alert(comp);
                $("#obtainMarks-" + k).val("");
                alert("Obtained Marks Should Be Greater than Equals to 33% ");
                return false;
            }

        }

    }
</script>
