@model PSEBONLINE.Models.Printlist
@{
    ViewBag.Title = "ExamFormCalFee";
    Layout = "~/Views/Shared/_Headerui.cshtml";
}
<style type="text/css">
    .nlink a {
        padding-right: 10px;
    }

    .input-validation-error {
        background-color: #FF9494 !important;
        border: solid 1px Red !important;
