                    // alert(response);
                }
            });
        }

        // function OnSuccess
        function OnSuccess(response) {
            var msg = $('#lblStatus');
            switch (response.oid) {
                case "0":
                    msg.css("slidedown", "slow");
                    msg.css("color", "red");
                    msg.text("School Code is Wrong.");
                    $("#tr1").hide();
                    break;
                case "1":
                    msg.css("slidedown", "slow");
                    msg.css("color", "green");
                    msg.text("School Code is Correct and Able for login.");
                    $("#schlname").val(response.sn);
                    $("#tr1").show();
                    break;
            }
        }



