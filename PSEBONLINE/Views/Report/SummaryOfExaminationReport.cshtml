@model PSEBONLINE.Models.ReportModel
@{
    ViewBag.Title = "Examination Report";
    Layout = "~/Views/Shared/_Card_Header.cshtml";
}
<style type="text/css">
    td {
        font-size: 11px;
        padding: 1px;
        vertical-align: top
    }
</style>
<link rel="stylesheet" href="https://registration2022.pseb.ac.in/Content/Print-4L.css" />
<h1 class="noprint">Summary of Examination Report</h1>
<div class="row">
    <div class="col-md-12 text-right noprint">
        <input type="button" value="Print" class="btn" onclick="window.print()" />
        <input type="button" value="Back" class="btn" onclick="history.go(-1);" />
    </div>
    <div class="clearfix"></div>
    <div class="col-md-12">
        <table style="border-collapse:collapse;width:100%;font-family:Arial, Helvetica, sans-serif;font-size:12px" border="1">
            <tr>
                <td style="border-right:0 !important">
                    <div style="padding:2px"><img src="~/Images/Printlogo.jpg" width="60px" height="60px" alt="PSEB" /></div>
                </td>
                <td colspan="8" style="font-size:16px;text-align:center;line-height:22px;vertical-align:top;border-left:0 !important;border-right:0 !important;padding-top:8px;">
                    <span style="font-size:28px">Punjab School Education Board</span>
                    <br />
                    <span>Summary of Examination Report Session (@Session["Session"].ToString().Trim())</span>
                </td>
                <td colspan="2" style="border-left:0 !important" align="right">Print Date : @DateTime.Today.ToString("dd/MM/yyyy") &nbsp;</td>
            </tr>
            <tr>
                <td width="15%"></td>
                <td width="8%">
                    Total Number of Candidates Registered
                </td>
                <td width="8%">
                    Total Number of Schools
                </td>
                <td width="10%">
                    Number of Candiates Selected
                </td>

                <td width="9%">
                    Number of Schools Selected
                </td>
                <td width="8%">
                    Total Challan Generated Student
                </td>
                <td width="8%">
                    Total Challan Generated Schools
                </td>
                <td width="9%">
                    Final Submission of candidates
                </td>
                <td width="9%">
                    Final Submission of Schools
                </td>
                <td width="8%">
                    Total Challan Generated
                </td>
                <td width="8%">
                    Total Challan Verified
                </td>
                @*<td width="7%">
            Total Fees
        </td>
        <td width="8%">
            Total Fees Verified
        </td>*@
            </tr>
            @if (ViewBag.TotalCount > 0)
            {

        <tr>
