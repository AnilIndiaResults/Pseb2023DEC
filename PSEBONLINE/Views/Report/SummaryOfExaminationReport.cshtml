            <td>@Model.StoreAllData.Tables[1].Rows[0]["tot_Num_Sel_candi"].ToString()</td>
            <td>@Model.StoreAllData.Tables[3].Rows[0]["tot_Sel_SCHL"].ToString()</td>

            <td>@Model.StoreAllData.Tables[8].Rows[0]["tot_Chln_gen_NOC"].ToString()</td>
