@{
    ViewBag.Title = "Month Wise Category Wise Fee Collection Details";
    Layout = "~/Views/Shared/_Card_Header.cshtml";
}
<link rel="stylesheet" href="https://registration2022.pseb.ac.in/Content/cardprintr.css" />
<h1 class="noprint">Month Wise Category Wise Fee Collection Details</h1>
<div class="row">
    <div class="col-md-12 noprint">
        @using (Html.BeginForm("MonthWiseCategoryWiseFeeCollectionDetails", "Report", FormMethod.Post, new { id = "searchform", enctype = "multipart/form-data" }))
        {
            if (Model != null)
            {
        <fieldset style="margin-bottom:15px">
            <legend>Search</legend>
            <div class="col-md-2">
                Bank @Html.DropDownList("Bank", new SelectList(ViewBag.MyBank, "Value", "Text", ViewBag.SelectedBank), "--All Banks---", new { @class = ""})
            </div>            
            <div class="col-md-2">
               Year/Month  @Html.DropDownList("YearMonth", new SelectList(ViewBag.YearList, "Value", "Text", ViewBag.SelDisplayYearMonth), "--All---", new { @class = "", })
            </div>
            <div class="col-md-6">
                Fee Category
                @*@Html.DropDownList("FEECAT", new SelectList(ViewBag.MySch, "Value", "Text", ViewBag.SelectedItem), "--All---", new { @class = "", id = "SelectItem" })*@
                @Html.ListBox("FEECAT", new SelectList(ViewBag.MySch, "Value", "Text", ViewBag.SelectedItem), new { id = "FEECAT", @class = "selectpicker show-tick form-control input-md" })
            </div>


            <div class="col-md-2"><br /><input type="submit" value="Export To Excel" name="submit" class="btn" /></div>
        </fieldset>
            }
        }
    </div>
    </div>
