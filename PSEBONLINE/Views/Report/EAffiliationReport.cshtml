@model PSEBONLINE.Models.ReportModel
@{
    ViewBag.Title = "EAffiliation Report";
    Layout = "~/Views/Shared/_Header.cshtml";
}
<link rel="stylesheet" href="https://registration2022.pseb.ac.in/Content/cardprintc.css" />
<h1 class="noprint">EAffiliation Summary Report</h1>
<div class="row">
    <div class="col-md-12 text-right noprint">
        <input type="button" value="Print" class="btn" onclick="window.print()" />
        <input type="button" value="Back" class="btn" onclick="history.go(-1);" />
    </div>
    <div class="clearfix"></div>

    <div class="col-md-12">
        <div style="border:1px solid #fff !important;display:block;margin-bottom:30px">
            <table border="1" width="100%" class="tprint">
                <thead>
                    <tr>
                        <td style="border-right:0 !important" width="15%"><div style="padding:2px"><img src="/Images/Printlogo.jpg" alt="PSEB" width="60px" height="60px"></div></td>
                        <td colspan="2" style="font-size:16px;text-align:center;line-height:24px;vertical-align:top;border-left:0 !important;border-right:0 !important;padding-top:8px">
                            <span style="font-size:28px">Punjab School Education Board</span><br><span>Count of schools applied for affiliation of 5th & 8th Class	</span>
                        </td>
                        <td style="text-align:right;border-left:0 !important;" width="15%">Print Date/Time : @DateTime.Now.ToString("dd/MM/yyyy hh:mmtt") &nbsp;</td>
                    </tr>
                    <tr>
                        <td width="15%">&nbsp;Descreption</td>
                        <td width="35%">&nbsp;Total Registered</td>
                        <td width="35%">&nbsp;Challan Generated</td>
                        <td width="15%">&nbsp;Fee Verified</td>
                    </tr>
                </thead>

                <tbody>
                    @{
                        if (ViewBag.TotalCount > 0)
                        {
                            int d = 0;
                            int Count = 0;

                            for (int i = 0; i < Model.StoreAllData.Tables[0].Rows.Count; i++)
                            {
                                d = d + 1;
                                var Class = Model.StoreAllData.Tables[0].Rows[i]["Class"].ToString();
                                var Total = Model.StoreAllData.Tables[0].Rows[i]["Total"].ToString();
                                var ChallanGenerated = Model.StoreAllData.Tables[0].Rows[i]["ChallanGenerated"].ToString();
                                var FeeVerified = Model.StoreAllData.Tables[0].Rows[i]["FeeVerified"].ToString();
 
                                Count = Count + 1;
                                <tr>
                                    <td>&nbsp;@Class</td>
                                    <td>&nbsp;@Total</td>
                                    <td>&nbsp;@ChallanGenerated</td>
                                    <td>&nbsp;@FeeVerified</td>               
                                </tr>
                            }
                        }
                        else
                        {
                            <tr>
                                <td colspan="4">
                                    <span class="rqf">Record Not Found</span>
                                </td>
                            </tr>
                        }
                    }
                </tbody>
            </table>
        </div>
    </div>

</div>



