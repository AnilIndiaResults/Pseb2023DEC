            //You an adopt max 3 languages
            <script type="text/javascript" language="javascript">
                alert("You cannot select more than three language");
                 window.location.href = '@Url.Action("Subjects", "Open")';
            </script>
        }
        else if (ViewData["Result"].ToString() == "5")
        {
            <script type="text/javascript" language="javascript">
                alert("Duplicate Subjects");
                window.location.href = '@Url.Action("OpenSubjectCorrectionPerforma", "CorrectionSubjects")';
            </script>
        }
        else if (ViewData["Result"].ToString() == "12NA" || ViewData["Result"].ToString() == "10NA")
        {
            <script type="text/javascript" language="javascript">
                alert("Not Allowed for CC");
               window.location.href = '@Url.Action("OpenSubjectCorrectionPerforma", "CorrectionSubjects")';
            </script>
        }
        else if (ViewData["Result"].ToString() == "AY")
        {
            <script type="text/javascript" language="javascript">
                alert("Please Select Appearing Year");
                window.location.href = '@Url.Action("OpenSubjectCorrectionPerforma", "CorrectionSubjects")';
            </script>
        }
        else if (ViewData["Result"].ToString() == "0")
        {
            <script type="text/javascript" language="javascript">
                alert("Correction data already exist for this student.");
                window.location.href = '@Url.Action("OpenSubjectCorrectionPerforma", "CorrectionSubjects")';
            </script>

        }
        else
        {
            <script type="text/javascript" language="javascript">
                alert("Subject Saved Failure, Plz Try Again ");
            </script>

        }

