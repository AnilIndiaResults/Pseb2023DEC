            <script type="text/javascript" language="javascript">

                alert("Correction data already exist for this student, please verify or reject correction then you can able to re-apply.");
            </script>
        }
        else if (result == "-3")
        {
            <script type="text/javascript" language="javascript">
                alert("Duplicate Aadhar number..plz try again");
            </script>
        }
        else if (result == "10")
        {
            <script type="text/javascript" language="javascript">

                 alert('@TempData["Duplicate"]');
            </script>
        }
        else if (result == "20")
        {
            <script type="text/javascript" language="javascript">
                alert('@TempData["errorMsg"]');
            </script>
        }
        else if (result == "15")
        {
            <script type="text/javascript" language="javascript">
                alert("Please fill mandatory subjects and check combination");
            </script>
        }
        else if (result == "NSQFSUBWANT")
        {
            <script type="text/javascript" language="javascript">
                alert('NSQF Subject Missing in Subject List');
            </script>
        }
        if (result == "5")
        {
            <script type="text/javascript" language="javascript">

                alert("Correction data already exist for this student, please verify or reject correction then you can able to re-apply.");
            </script>
        }

        else if (result == "3")
        {
            <script type="text/javascript" language="javascript">

                alert("Correction data already exist for this student.");
            </script>
        }
        else if (result == "2")
        {
            <script type="text/javascript" language="javascript">

                alert("Correction data can not same, please check.");
            </script>
        }
        else if (result == "1")
        {
            <script type="text/javascript" language="javascript">
                alert("Correction data Added successfully.");
            </script>
        }
        else if (result == "0")
        {
            <script type="text/javascript" language="javascript">
                alert("Correction data not Added, Please try again .");
            </script>
        }
    }

