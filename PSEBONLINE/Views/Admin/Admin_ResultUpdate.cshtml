@{
    Layout = "~/Views/Shared/_Header.cshtml";
}


<div class="container KDBG">
    <style type="text/css">
        #ac-wrapper {
            background: rgba(0, 0, 0, 0.5) none repeat scroll 0 0;
            height: 100%;
            left: 0;
            position: fixed;
            top: 0;
            width: 100%;
            z-index: 1001;
        }

        #popup {
            min-height: 240px;
            left: 0;
            padding: 0;
            position: absolute;
            top: 128px;
            width: 100%;
        }

            #popup table {
                background: #fff none repeat scroll 0 0;
                border: 5px solid #f1f1f1;
                border-radius: 0;
                box-shadow: 0 0 3px 3px #64686e;
                max-width: 500px;
                min-width: 320px;
            }

            #popup .button {
                padding: 10px;
                background: #efba09; /* Old browsers */
                background: -moz-linear-gradient(top, #efba09 0%, #fb9709 50%, #efba09 100%); /* FF3.6-15 */
                background: -webkit-linear-gradient(top, #efba09 0%,#fb9709 50%,#efba09 100%); /* Chrome10-25,Safari5.1-6 */
                background: linear-gradient(to bottom, #efba09 0%,#fb9709 50%,#efba09 100%); /* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
                filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#efba09', endColorstr='#efba09',GradientType=0 ); /* IE6-9 */
            }
    </style>

    <style type="text/css">
        .ullist li {
            padding: 6px 4px;
            font-size: 16px;
        }

        .ullist {
            padding-bottom: 10px;
        }
    </style>
    <h1>Admin Result updation Panel for <b>@Session["Session"].ToString()</b></h1>
    <div class="row">
        <div class="col-md-12">
            <ul class="ullist">

                @if (ViewBag.NApprovedSchl == "1")
                {
                    <li><a href="/Admin/Admin_Ninth_Result_Page"><samp>&#10093;</samp>  9th Result Updation (Session <b>@Session["Session"].ToString()</b>)</a></li>
                }
                @if (ViewBag.EApprovedSchl == "1")
                {
                    <li><a href="/Admin/Admin_Eleventh_Result_Page"><samp>&#10093;</samp> 11th Result Updation (Session <b>@Session["Session"].ToString()</b>)</a></li>
                }
                </ul>
            
</div>
    </div>


</div>



