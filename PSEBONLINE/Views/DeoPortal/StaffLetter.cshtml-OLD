<script type="text/javascript">

    function dee() {

        if ($("#SelDist").val() == "") {

            $("#reqsession").slideDown("slow");
            return false;
        }
        if ($("#SelDist").val() == "ALL" && ($("#Category").val() == "0" || $("#Category").val() == "")) {
            //alert("Please Select One Category");
            $("#reqcatglist").slideDown("slow");
            return false;
        }
        if ($("#Category").val() != "0" && $("#SearchString").val() == "") {

            $("#reqsellist").slideDown("slow");
            return false;
        }

            //else if ($("#SelDist").val() != "" && $("#Category").val() != "" && $("#SearchString").val() == "") {
            //    $("#reqsellist").slideUp("slow");
            //    $("#reqsstring").slideDown("slow");
            //    return false;
            //}
        else
            return true;

    }

    function catchange() {
        if ($("#Category").val() == "0" || $("#Category").val() == "") {
            $("#reqcatglist").slideDown("slow");
            return false;
        }
        else {
            $("#reqcatglist").slideUp("slow");
        }
        return true;

    }
   
    function deechange() {
        if ($("#SelDist").val() == "") {
            $("#reqsession").slideDown("slow");
            return false;
        }
        else {
            $("#reqsession").slideUp("slow");
        }
       //if ($("#SelDist").val() != "ALL" || ($("#SelDist").val() != "0")) {
       //     $("#reqcatglist").slideUp("slow");
       //     return false;
       // }
        return true;

    }
    


