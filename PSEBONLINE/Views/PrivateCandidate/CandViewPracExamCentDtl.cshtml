                    @Html.TextBoxFor(m => m.SearchString, new { id = "SearchString", @maxlength = "15", onkeypress = "return isNumberKey(event);", placeholder = "Entre your Ref_No or Roll no." })
                </div>
                <div class="col-md-2">
                    &nbsp;<br />
