<div class="container KDBG">
    @*<h1>Matric/Senior Secondary Supplementary Exam (Reap/Additional), October-2017</h1>*@
    @using (Html.BeginForm("PrivateCandidateConfirmation", "PrivateCandidate", FormMethod.Post, new { id = "Oroll", enctype = "multipart/form-data" }))
    {
        var refnoH = Session["refno"].ToString().Substring(3, 2);
        var yearH = "20" + Session["refno"].ToString().Substring(5, 2);
        if (refnoH == "03")
        {
            refnoH = "March";
            <h1 class="noprint">Matric/Sr. Secondary Supplementary Exam (Reap/Compartment/DIC), @refnoH -@yearH</h1> }
        else if (refnoH == "07")
        {
            refnoH = "July";
            <h1 class="noprint">Matric/Sr. Secondary Supplementary Exam (Reap/Compartment/Additional), @refnoH-@yearH</h1>
        }
        else if (refnoH == "08")
        {
            refnoH = "August";
            <h1 class="noprint">Matric/Sr. Secondary Supplementary Exam (Reap/Compartment/Additional), @refnoH-@yearH</h1>
        }
        else if (refnoH == "09")
        {
            refnoH = "September";
            <h1 class="noprint">Matric/Sr.Secondary Supplementary Golden Chance Exam (Reap/Improvement/Additional), @refnoH-@yearH</h1>
        }
        else if (refnoH == "10")
        {
            refnoH = "October";
            <h1 class="noprint">Matric/Sr. Secondary Supplementary Exam (Reap/Compartment), @refnoH -@yearH</h1> }

