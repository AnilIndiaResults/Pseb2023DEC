            <input type="submit" id="btnsubmit" value="Update Candidate Details" onclick="return valForm();" />
            <input type="button" id="btnOTP" value="Update Candidate Details With Correction" onclick="return sendOTP();" />


            <input type="button" value="Add New Records" onclick="javascript:document.location.href='@Url.Action("N1Form", "RegistrationPortal")'" />
            <input type="button" formmethod="post" value="View Inserted Records" onclick="javascript:document.location.href='@Url.Action("N1Formgrid", "RegistrationPortal")'" />

