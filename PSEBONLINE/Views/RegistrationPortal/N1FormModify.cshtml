                    District <span class="rqf">*</span>
                </div>
                <div class="col-md-4">
                    @Html.DropDownList("MyDistrict", new SelectList(ViewBag.MyDist, "Value", "Text"), new { id = "MyDist", @class = "", @readonly = "readonly" })
                </div>
                <div class="clearfix"></div>
                <div class="col-md-2">
                    Tehsil <span class="rqf">*</span>
                </div>
                <div class="col-md-4">
