@{
    if (ViewData["resultUpdate"] != null)
    {
        @*if (ViewData["resultUpdate"].ToString() == "")
            {
                <script type="text/javascript" language="javascript">
                alert("Data Updated Successfully");
                window.location.href = '@Url.Action("N1Formgrid", "RegistrationPortal")';
                </script>
            }*@
        if (ViewData["resultUpdate"].ToString() == "0")
        {
            <script type="text/javascript" language="javascript">
                alert("Data Already Exist");
            </script>
        }
        else if (ViewData["resultUpdate"].ToString() == "-1")
        {
            <script type="text/javascript" language="javascript">
                alert("Data not saved..plz try again");
            </script>
        }
        else if (ViewData["resultUpdate"].ToString() == "-3")
        {
            <script type="text/javascript" language="javascript">
                alert("Duplicate Aadhar number..plz try again");
            </script>
        }
        else
        {
            <script type="text/javascript" language="javascript">
                    alert("Data Updated Successfully");
                    window.location.href = '@Url.Action("N1Formgrid", "RegistrationPortal")';
            </script>

        }

