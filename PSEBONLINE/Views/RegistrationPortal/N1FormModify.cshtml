    <div class="clearfix"></div>
    <h4>Fill Address</h4>
    <div class="row">
        <div class="col-md-8">
            <div class="row">
                <div class="col-md-2">
                    Address <span class="rqf">*</span>
                </div>
                <div class="col-md-10">@Html.TextBoxFor(a => a.Address, new { id = "txt_address", @maxlength = "250" })</div>
                <div class="clearfix"></div>
                <div class="clearfix"></div>
                <div class="col-md-2">
                    Landmark
                </div>
                <div class="col-md-10">@Html.TextBoxFor(a => a.LandMark, new { id = "Landmark", @maxlength = "50" })</div>
                <div class="clearfix"></div>
                <div class="col-md-2">
                    Block
                </div>
                <div class="col-md-4">
                    @Html.TextBoxFor(a => a.Block, new { @maxlength = "50" })
                </div>
