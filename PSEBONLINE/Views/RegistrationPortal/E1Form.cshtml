        var file1 = document.querySelector('#fu_photo')

        if (regex.test($(file1).val().toLowerCase())) {
            var s = file1.files[0];

            if (s.size > 2048 && s.size < 51200) {

                var file = document.querySelector('#fu_photo').files[0];
                var reader = new FileReader();

                reader.onloadend = function () {
                    preview.src = reader.result;
                    setTimeout(fdetect, 1000);

                }

                if (file) {

                    reader.readAsDataURL(file);

                    //ode here

                } else {
                    preview.src = "";
                }
            }
            else {

                preview.src = "";

                $('#fu_photo').val("");
                alert("File Size Should be 2 to 50 KB");
            }
        }
        else {

            var control = $('#fu_photo').val();
            $('#fu_photo').val("");


            preview.src = "";

            alert("Please upload a valid image file.");
        }

    }

    // facedetection function

    function fdetect() {

        $('img').each(function () {
            var img = this;

            // Get faces cooridnates
            var coords = "";
            coords = $(img).faceDetection({

                complete: function (image, coords) {
                    // Do something

                    // Make boxes if faces are found
                    if (coords.length) {
                        document.getElementById('errorDiv').style.visibility = 'hidden';
                        document.getElementById('successdiv').style.visibility = 'visible';
                        alert("Valid Image");

                    }
                    else {
                        document.getElementById('errorDiv').style.visibility = 'visible';
                        document.getElementById('successdiv').style.visibility = 'hidden';
                        alert("Invalid Image");
                    }

                },
                error: function () {
                    alert("Could not process image");
                }
            });
        });
    }

</script>


