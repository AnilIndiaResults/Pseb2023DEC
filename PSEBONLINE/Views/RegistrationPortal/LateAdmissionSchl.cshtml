    <div class="row">
        <div class="col-md-12 noprint">
            <fieldset style="margin-bottom:15px">
                <legend>Search</legend>
                <div class="col-md-2">
                    Total Students <label>@ViewBag.TotalCount</label>
                </div>
                <div class="col-md-3">
