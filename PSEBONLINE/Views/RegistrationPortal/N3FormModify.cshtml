            alert("Invalid character...Enter Only Number!!");
            return false;
        }
    }


    function isNumberOnly(e) {
        var keyCode = (e.which) ? e.which : e.keyCode;
        if (keyCode > 31 && (keyCode < 48 || keyCode > 57)) {
            alert("Only Number Allowed!!");
            return false;
        }
        return true;
    }


