    function EpubjabStaffIdAvailability() { //This function call on text change.
        document.getElementById("dvstaffdetails").style.visibility = 'hidden';
        $('#loading10').html("<p><img src='https://registration2022.pseb.ac.in/images/loadingk.gif'></p>");
        $.ajax({
            type: "POST",
            url: "/RegistrationPortal/GetStaffDetailsByEpunjjabId", // this for calling the web method function in cs code.
            data: '{SchoolHeadEPunjabId: "' + $("#StaffId").val() + '" }',// user name or email value
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: OnSuccessEpubjabStaffId,
            failure: function (response) {
                // alert(response);
            }
        });
    }

    // function OnSuccess
    function OnSuccessEpubjabStaffId(response) {
        document.getElementById("dvstaffdetails").style.visibility = 'visible';
        $('#loading10').empty();
        var msg = $('#lblStatus');
        switch (response.oid) {
            case "0":
                msg.css("slidedown", "slow");
                msg.css("color", "red");
                msg.text("Not found.");
                $("#EPunjabTeacherName").val('');
                $("#EPunjabTeacherMobile").val('');
                break;
            case "1":
                msg.css("slidedown", "slow");
                msg.css("font-size", "18px");
                msg.css("color", "green");
                $("#EPunjabTeacherName").val(response.name);
                $("#EPunjabTeacherMobile").val(response.mob);
                $("#EPunjabTeacherName").hide();
                $("#EPunjabTeacherMobile").hide();
                //document.getElementById("btnsubmit").style.display = 'none';
                //document.getElementById("btnOTP").style.display = 'block';

                $("#btnsubmit").hide();
                $("#btnOTP").show();
                msg.text("Name: " + response.name + " , Mobile: " + response.mob.replace(/\d(?=\d{4})/g, "X"));
                break;
        }
    }



</script>


<script type="text/javascript">



    function sendOTP() {

        var valstatus = valForm();

        if (valstatus) {
            var eTeacherId = $("#StaffId").val();
          //  alert(eTeacherId);
            var eTeacherName = $("#EPunjabTeacherName").val();
           // alert(eTeacherName);
            var eTeacherMobile = $("#EPunjabTeacherMobile").val();
           // alert(eTeacherMobile);
            var eTeacherMobileOTP = "";
            var std_id = '@Model.Std_id';

            //alert('Send OTP');
            $('#loadingSO').html("<p><img src='https://registration2022.pseb.ac.in/images/loadingk.gif'></p>");
            $.ajax({
                type: "POST",
                url: "/RegistrationPortal/TeacherMobileOTP", // this for calling the web method function in cs code.
                data: '{eTeacherId: "'+ eTeacherId+ '",eTeacherName: "' + eTeacherName + '",eTeacherMobile: "' + eTeacherMobile + '",eTeacherMobileOTP: "' + eTeacherMobileOTP+ '",std_id: "' + std_id + '", "Type": "SEND"}',
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (response) {
                    $('#loadingSO').empty();
                    if (response.outstatus == "1") {
                        alert("OTP Send Successfully");

                        $("#eTeacherName").val(response.name);
                        $("#eTeacherMobile").val(response.mob.replace(/\d(?=\d{4})/g, "X"));
                        $("#eTeacherMobilehf").val(response.mob);

                        //$("#eTeacherMobileOTP").val(response.otp);
                        $('#myModalOTP').modal('show');
                        //.modal('show');
                    }
                    else  {
                        alert("OTP Verified Failure");
                    }


                },
                failure: function (response) {
                    alert(response);
                }
            });
        }
        return false;
    }


    function VerifyTeacherOTP(std_id) {

        var flag = 0;
        if ($("#eTeacherName").val() == "") {
            $("#eTeacherName").addClass("input-validation-error");
            flag = 1;
        }
        if ($("#eTeacherMobile").val() == "") {
            $("#eTeacherMobile").addClass("input-validation-error");
            flag = 1;
        }
        if ($("#eTeacherMobileOTP").val() == "") {
            $("#eTeacherMobileOTP").addClass("input-validation-error");
            flag = 1;
        }


        if (flag == 1) {
            return false;
        }
        else {
            var eTeacherId = $("#StaffId").val();
           // alert(eTeacherId);
            var eTeacherName = $("#eTeacherName").val();
           // alert(eTeacherName);
            var eTeacherMobile = $("#eTeacherMobilehf").val();
          // alert(eTeacherMobile);
            var eTeacherMobileOTP = $("#eTeacherMobileOTP").val();
           // alert(eTeacherMobileOTP);
            var std_id = '@Model.Std_id';


            $('#loading1').html("<p><img src='https://registration2022.pseb.ac.in/images/loadingk.gif'></p>");
            $.ajax({
                type: "POST",
                url: "/RegistrationPortal/TeacherMobileOTP", // this for calling the web method function in cs code.
                data: '{eTeacherId: "' + eTeacherId + '",eTeacherName: "' + eTeacherName + '",eTeacherMobile: "' + eTeacherMobile + '",eTeacherMobileOTP: "' + eTeacherMobileOTP + '",std_id: "' + std_id + '", "Type": "VERIFIY"}',
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (response) {
                    $('#loading1').empty();
                    if (response.outstatus == "1") {
                        alert("OTP Verified Successfully");
                        $("#eTeacherMobileOTP").val('');
                        $('#myModalOTP').modal('hide');
                        $('#searchform').append('<input type="hidden" id="hfIsCorrectionInParticular" name="hfIsCorrectionInParticular " value="Y" />');
                        $('#searchform').submit();
                    }
                    else if (response.outstatus == "2") {
                        alert("Wrong OTP");
                    }
                    else if (response.outstatus == "0") {
                        alert("OTP Verified Failure");
                    }
                },
                failure: function (response) {
                    alert(response);
                }
            });
            //alert("All is Well");
        }

    }
</script>
