    // function OnSuccess
    function OnSuccessEpubjabStaffId(response) {
        document.getElementById("dvstaffdetails").style.visibility = 'visible';
        $('#loading10').empty();
        var msg = $('#lblStatus');
        switch (response.oid) {
            case "0":
                msg.css("slidedown", "slow");
                msg.css("color", "red");
                msg.text("Not found.");
                $("#EPunjabTeacherName").val('');
                $("#EPunjabTeacherMobile").val('');
                break;
            case "1":
                msg.css("slidedown", "slow");
                msg.css("font-size", "18px");
                msg.css("color", "green");
                $("#EPunjabTeacherName").val(response.name);
                $("#EPunjabTeacherMobile").val(response.mob);
                $("#EPunjabTeacherName").hide();
                $("#EPunjabTeacherMobile").hide();
                //document.getElementById("btnsubmit").style.display = 'none';
                //document.getElementById("btnOTP").style.display = 'block';
                $("#btnsubmit").hide();
                $("#btnOTP").show();
                msg.text("Name: " + response.name + " , Mobile: " + response.mob.replace(/\d(?=\d{4})/g, "X"));
                break;
        }
    }



</script>


