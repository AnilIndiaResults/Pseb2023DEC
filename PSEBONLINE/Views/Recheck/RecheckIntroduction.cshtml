@{

    if (ViewData["Status"] != null)
    {
        var result = ViewData["Status"].ToString();
        if (result == "0")
        {
            <script type="text/javascript" language="javascript">
                alert("Refrence No. Not Generated. Please check details ");
                //document.location.href = 'Url.Action("RecheckExamination", "Recheck")';
            </script>
        }
        else if (result == "1")
        {
            <script type="text/javascript" language="javascript">
                @*document.location.href = '@Url.Action("RecheckConfirmation/4R0317200001", "Recheck")';*@
                var refno = @TempData["refno"].ToString();
                var roll = @TempData["roll"].ToString();
                @*var Yar = @TempData["Yar"].ToString();
                var Mnth = @TempData["Mnth"].ToString();*@

                alert("You are successfully registred, your refrence no. " + refno + " is generated against roll no. " + roll + ", Keep this for further use till result declaration.");
                document.location.href = '@Url.Action("RecheckConfirmation/" + @TempData["refno"].ToString(), "Recheck")';
            </script>
        }

        else if (result == "2")
        {
            <script type="text/javascript" language="javascript">
                alert("Refrence No.: "+@ViewData["RefNo"] +" Already Generated. Kindly Check your Email and Mobile No. ");
                @*document.location.href = '@Url.Action("RecheckConfirmation/" + ViewData["roll"], "Recheck")';*@
            </script>
        }
        else if (result == "-10")
        {
            <script type="text/javascript" language="javascript">
                alert("Last Date of Rechecking/Re-evaluation are Finished");
                @*document.location.href = '@Url.Action("RecheckConfirmation/" + ViewData["roll"], "Recheck")';*@
            </script>
        }
        else if (result == "-1")
        {
            <script type="text/javascript" language="javascript">
                alert("RollNo Does not Exists, Kindly Check your Roll Number");
            </script>
        }


    }

