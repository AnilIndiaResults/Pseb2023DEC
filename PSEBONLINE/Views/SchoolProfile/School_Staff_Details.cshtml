<script>
    function validateStd(event) {
        var key = window.event ? event.keyCode : event.which;
        var c = $("#stdno").val();
        if (event.keyCode === 8 || event.keyCode === 46
            || event.keyCode === 37 || event.keyCode === 39) {
            return true;
        }
        if (key < 48 || key > 57) {
            return false;
        }
        if (c.length > 3 && c.length < 5) {
            return true;
        }
        if (c.length > 5) {
            return false;
        }

        else return true;
    }
    function validatePhone(event) {
        var key = window.event ? event.keyCode : event.which;
        var c = $("#phoneno").val();
        if (event.keyCode === 8 || event.keyCode === 46
            || event.keyCode === 37 || event.keyCode === 39) {
            return true;
        }
        if (key < 48 || key > 57) {
            return false;
        }
        if (c.length > 5 && c.length < 6) {
            return true;
        }
        if (c.length > 7) {
            return false;
        }

        else return true;
    }
    function validatePin(event) {
        var key = window.event ? event.keyCode : event.which;
        var c = $("#pincid").val();
        if (event.keyCode === 8 || event.keyCode === 46
            || event.keyCode === 37 || event.keyCode === 39) {
            return true;
        }
        if (key < 48 || key > 57) {
            return false;
        }
        if (c.length > 5) {
            return false;
        }

        else return true;
    }
    function validateAadhar(event) {
        var key = window.event ? event.keyCode : event.which;
        var c = $("#aadno").val();
        if (event.keyCode === 8 || event.keyCode === 46
            || event.keyCode === 37 || event.keyCode === 39) {
            return true;
        }
        if (key < 48 || key > 57) {
            return false;
        }
        if (c.length > 11) {
            return false;
        }

        else return true;
    }

    function validateMonth(event) {
        var key = window.event ? event.keyCode : event.which;
        var c = $("#ExpMonth").val();
        //alert(c);
        if (event.keyCode === 8 || event.keyCode === 46
            || event.keyCode === 37 || event.keyCode === 39) {
            return true;
        }
        if (key < 48 || key > 57) {
            return false;
        }
        if (c > 11) {
            $("#ExpMonth").val("");
            alert('Please enter maximum 11 month other wise enter in year');
            return false;
        }
        if (c.length > 1) {
            return false;
        }


        else return true;
    }
    function validateYear(event) {
        var key = window.event ? event.keyCode : event.which;
        var c = $("#ExpYear").val();
        if (event.keyCode === 8 || event.keyCode === 46
            || event.keyCode === 37 || event.keyCode === 39) {
            return true;
        }
        if (key < 48 || key > 57) {
            return false;
        }
        if (c.length > 1) {
            return false;
        }

        else return true;
    }
</script>
