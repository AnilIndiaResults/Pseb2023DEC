<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
<link rel="stylesheet" href="https://code.jquery.com/ui/1.12.0/themes/base/jquery-ui.css">
<div id="dialogREG" style="display: none">
    <span class="rqf">Has this candidate ever been allotted a board registration number in a class? ਕੀ ਇਸ ਪ੍ਰੀਖਿਆਰਥੀ ਨੂੰ ਕਦੇ ਕਿਸੇ ਕਲਾਸ ਵਿੱਚ ਬੋਰਡ ਦਾ ਰਜਿਸਟਰੇਸ਼ਨ ਨੰਬਰ ਅਲਾਟ ਹੋਇਆ ਹੈ?</span>
</div>
<div id="dialogREGNew" style="display: none">
    <span class="rqf">Has this candidate ever been allotted a board registration number in a class? ਕੀ ਇਸ ਪ੍ਰੀਖਿਆਰਥੀ ਨੂੰ ਕਦੇ ਕਿਸੇ ਕਲਾਸ ਵਿੱਚ ਬੋਰਡ ਦਾ ਰਜਿਸਟਰੇਸ਼ਨ ਨੰਬਰ ਅਲਾਟ ਹੋਇਆ ਹੈ?</span>
</div>
<script type="text/javascript">


    $("#btnSubmitForm").click(function () {
        $('#dialogREG').dialog('open');
    });



    $(function () {
        $("#dialogREG").dialog({
            modal: true,
            autoOpen: false,
            title: "Has this candidate ever been allotted a board registration number in a class?",
            width: 600,
            height: 300,
            buttons: {
                "NO": function () {
                    $("#btnSubmitForm").attr('disabled', 'disabled');
                    $(this).dialog("close");
                    window.location.href = "/Webservice/Index";
                },
                "YES": function () {
                    alert("Kindly add/import student data in N3 forms");
                    $(this).dialog("close");
                },
                Cancel: function () {
                    $(this).dialog("close");
                }
            }
        });
    });



    $("#btnSubmitFormNew").click(function () {
        $('#dialogREGNew').dialog('open');
    });



    $(function () {
        $("#dialogREGNew").dialog({
            modal: true,
            autoOpen: false,
            title: "Has this candidate ever been allotted a board registration number in a class?",
            width: 600,
            height: 300,
            buttons: {
                "NO": function () {
                    $("#btnSubmitForm").attr('disabled', 'disabled');
                    $(this).dialog("close");
                    window.location.href = "/RegistrationPortal/N1Form";
                },
                "YES": function () {
                    alert("Kindly add/import student data in N3 forms");
                    $(this).dialog("close");
                },
                Cancel: function () {
                    $(this).dialog("close");
                }
            }
        });
    });

</script>