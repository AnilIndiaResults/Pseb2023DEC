<script type="text/javascript">
    window.setInterval(function () {
        var current = new Date();
        var expiry = new Date("July 12, 2021 23:59:59")
        if (current.getTime() > expiry.getTime()) {
            $('.hideint').hide();
        } else if (current.getTime() < expiry.getTime()) {
            $('.hideint').show();
        }
    }, 0);
</script>
