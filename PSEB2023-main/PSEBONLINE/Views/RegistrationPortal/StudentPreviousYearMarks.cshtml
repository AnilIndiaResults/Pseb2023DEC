<script>
    function CheckMarks(obj) {
        //pad(obj, 3);
        return true;
    }


    function pad(obj, length) {
        var text = obj.value;
        var str = '' + text;
        while (str.length < length) {
            str = '0' + str;
        }
        if (obj.value == 'A' || obj.value == 'C' || obj.value == 'a' || obj.value == 'c') { }
        else {
            obj.value = str;
        }
