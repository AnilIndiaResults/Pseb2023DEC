@{
    if (ViewData["resultUpdate"] != null)
    {
        if (ViewData["resultUpdate"].ToString() == "1")
        {
            <script type="text/javascript" language="javascript">
                alert("Data Update Successfully");
                window.location.href = '@Url.Action("M1Formgrid", "RegistrationPortal")';
            </script>

        }
        else if (ViewData["resultUpdate"].ToString() == "-1")
        {
            <script type="text/javascript" language="javascript">
                alert("Data Already Exist");
            </script>
        }
        else if (ViewData["resultUpdate"].ToString() == "15")
        {
            <script type="text/javascript" language="javascript">
                alert("Please fill mandatory subjects and check combination");
            </script>
        }
        else if (ViewData["resultUpdate"].ToString() == "10")
        {
            <script type="text/javascript" language="javascript">
                alert('@ViewBag.Duplicate');
            </script>
        }
        else if (ViewData["resultUpdate"].ToString() == "NSQFSUBWANT")
        {
            <script type="text/javascript" language="javascript">
                alert('NSQF Subject Missing in Subject List');
            </script>
        }
        else
        {
            <script type="text/javascript" language="javascript">
                alert("Data not Update..plz try again");
            </script>
        }
    }
}

