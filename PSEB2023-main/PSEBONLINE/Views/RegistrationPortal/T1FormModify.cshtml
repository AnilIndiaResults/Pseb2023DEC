            $('#loading1').html("<p><img src='https://registration2022.pseb.ac.in/images/loadingk.gif'></p>");
            $.ajax({
                type: "POST",
                url: "/RegistrationPortal/TeacherMobileOTP", // this for calling the web method function in cs code.
