                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (response) {
                    $('#loading1').empty();
                    if (response.outstatus == "1") {
                        alert("OTP Verified Successfully");
                        $("#eTeacherMobileOTP").val('');
                        $('#myModalOTP').modal('hide');
                        $('#searchform').append('<input type="hidden" id="hfIsCorrectionInParticular" name="hfIsCorrectionInParticular " value="Y" />');
