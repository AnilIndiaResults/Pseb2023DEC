            flag = 1;
        }
        if ($("#eTeacherMobile").val() == "") {
            $("#eTeacherMobile").addClass("input-validation-error");
            flag = 1;
        }
        if ($("#eTeacherMobileOTP").val() == "") {
            $("#eTeacherMobileOTP").addClass("input-validation-error");
            flag = 1;
        }


        if (flag == 1) {
            return false;
        }
        else {
            var eTeacherId = $("#StaffId").val();
           // alert(eTeacherId);
            var eTeacherName = $("#eTeacherName").val();
           // alert(eTeacherName);
            var eTeacherMobile = $("#eTeacherMobilehf").val();
