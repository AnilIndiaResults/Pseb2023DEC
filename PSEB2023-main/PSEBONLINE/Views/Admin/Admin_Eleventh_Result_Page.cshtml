        if (nxt == "0" && prv == "0")
        { }
        else if (nxt == "0" && prv!="0")
        {
           $('#myModal1-' + prv).modal('show');
        }
        else if (prv == "0" && nxt != "0") {
            $('#myModal1-'+nxt).modal('show');
        }
        else if (prv != "0" && nxt != "0") {
            $('#myModal1-' + nxt).modal('show');
        }
    }


