            success: function (response) {
                $("#loadingSM" + k).empty();
                var msg = $('#lblStatus-' + k);
                msg.text("");
                switch (response.oid) {
                    case "0":
                        msg.css("slidedown", "slow");
                        msg.css("color", "red");
                        msg.text("Invalid School Code");
                        $("#dvschle-" + k).hide();
                        $("#btnupdateSchoolMaster-" + k).hide();
                        break;
                    case "1":
                        msg.css("slidedown", "slow");
                        msg.css("color", "green");
                        console.log(response);
                        $("#btnupdateSchoolMaster-" + k).show();
                        $("#SCHLE1-" + k).val(response.sm.SCHLE);
                        $("#SCHLP1-" + k).val(response.sm.SCHLP);
                        $("#STATIONE1-" + k).val(response.sm.STATIONE);
                        $("#STATIONP1-" + k).val(response.sm.STATIONP);
                        //class group details
                        $("#lblOLDPrimary-" + k).val(response.sm.fifth);
                        $("#lblOLDMiddle-" + k).val(response.sm.middle);
                        $("#lblOLDMatric-" + k).val(response.sm.MATRIC);
                        $("#lblOLDHumanities-" + k).val(response.sm.HUM);
                        $("#lblOLDScience-" + k).val(response.sm.SCI);
                        $("#lblOLDCommerce-" + k).val(response.sm.COMM);
                        $("#lblOLDVocational-" + k).val(response.sm.VOC);
                        $("#lblOLDAgriculture-" + k).val(response.sm.AGRI);
                        //
                        //class group details (NEW)
                        $("#lblNEwPrimary-" + k).val(response.eam.NEW_PRIMARY);
                        $("#lblNEwMiddle-" + k).val(response.eam.NEW_MIDDLE);
                        $("#lblNEwMatric-" + k).val(response.eam.NEW_MATRIC);
                        $("#lblNEwHumanities-" + k).val(response.eam.NEW_HUM);
                        $("#lblNEwScience-" + k).val(response.eam.NEW_SCI);
                        $("#lblNEwCommerce-" + k).val(response.eam.NEW_COMM);
                        msg.text("Valid School Code");
                        $("#dvschle-" + k).show();
                        break;
                }
            },
            failure: function (response) {
                // alert(response);
            }
        });
    }



</script>

