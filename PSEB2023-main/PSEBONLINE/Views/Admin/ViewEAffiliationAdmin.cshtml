                <div class="col-md-3">@Html.DropDownList("AppCls", new SelectList(ViewBag.MyAppCls, "Value", "Text", ViewBag.SelectedCls), "--All Class--", new { @class = "", id = "AppCls" })</div>
                <div class="col-md-3">@Html.DropDownList("AppType", new SelectList(ViewBag.MyAppType, "Value", "Text", ViewBag.SelectedAppType), "--Select App Type--", new { @class = "", id = "AppType", @required = "required" })</div>
