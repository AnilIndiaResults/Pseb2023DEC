    //updateSchoolMaster
    function updateSchoolMaster(k, APPNO) {

        var flag = 0;
        if ($("#SchlCat-" + k).val() == "") {
            $("#SchlCat-" + k).addClass("input-validation-error");
            flag = 1;
        }

        if ($("#SchlCat-" + k).val() == "Old") {
            if ($("#SCHL-" + k).val() == "") {
                $("#SCHL-" + k).addClass("input-validation-error");
                flag = 1;
            }
            if ($("#SCHLE-" + k).val() == "") {
                $("#SCHLE-" + k).addClass("input-validation-error");
                flag = 1;
            }
            if ($("#SCHLP-" + k).val() == "") {
                $("#SCHLP-" + k).addClass("input-validation-error");
                flag = 1;
            }
            if ($("#STATIONE-" + k).val() == "") {
                $("#STATIONE-" + k).addClass("input-validation-error");
                flag = 1;
            }
            if ($("#STATIONP-" + k).val() == "") {
