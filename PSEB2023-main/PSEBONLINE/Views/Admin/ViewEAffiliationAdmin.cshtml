<script type="text/javascript">
    function isNumberKey(evt) {
        var charCode = (evt.which) ? evt.which : event.keyCode
        if ((charCode >= 48 && charCode <= 57) || (charCode == 8) || (charCode == 46)) {
            return true;
        }
        else {
            alert("Invalid character...Enter Only Number!!");
            return false;
        }
    }
    $(document).ready(function () {

        $(".cschl").hide();
    });

    function GetSelectedTextValueNew(selNum1) {
        $(".cschl").hide();
        $("#btnupdateSchoolMaster-" + k).hide();
        var selectedText = selNum1.options[selNum1.selectedIndex].innerHTML;
        var value = selNum1.value;
        var k = selNum1.id.split('-')[1];
        var selectedCAT = value;
        if (selectedCAT == 'Old') {
            $("#dvoldschl-" + k).show();
            $("#SCHL-" + k).val("");
            $("#dvschle-" + k).hide();
            $("#btnupdateSchoolMaster-" + k).hide();

        }
        else {
            //$("#dvoldschl-" + k).hide();
            //$("#SCHL-" + k).val("");
            //$("#dvschle-" + k).hide();
            //$("#btnupdateSchoolMaster-" + k).show();

            $("#dvoldschl-" + k).show();
            $("#SCHL-" + k).val("NEW");
            $("#dvschle-" + k).hide();
            $("#btnupdateSchoolMaster-" + k).hide();          

        }
    }


</script>

<script type="text/javascript">


    function SchoolCodeAvailability(selNum1, APPNO) { //This function call on text change.
      
        var k = selNum1;
        var schlEntered = $("#SCHL-" + k).val();
        

        //var value = selNum1.value;
        //var k = selNum1.id.split('-')[1];
        var selectedID = schlEntered;
        $("#loadingSM" + k).html("<p><img src='https://registration2022.pseb.ac.in/images/loadingk.gif'></p>");
        $("#dvschle-" + k).hide();
        $.ajax({
            type: "POST",
            url: "../School/CheckSchoolCodeAndGetAllDetails", // this for calling the web method function in cs code.
