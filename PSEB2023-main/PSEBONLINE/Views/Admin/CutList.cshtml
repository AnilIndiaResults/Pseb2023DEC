@{
    ViewBag.Title = "CutList";
    Layout = "~/Views/Shared/_Header.cshtml";
}
<h1>Cut List Print Report</h1>
<div class="row">
    <div class="col-md-10">
        <div id="loading-image" style="color:#cc0000;font-size:16px">Downloading Cut-List Please wait <img src="/images/fbloader.gif" /></div>

    </div>
    <div class="col-md-2 text-center">
        <input type="button" value="Back" onclick="javascript:document.location.href='@Url.Action("AdminCutList", "Admin" )'" />
    </div>
     
            <div class="col-md-12">
                <ul class="ullist">
                    @if (ViewBag.CutlistClass == "M" || ViewBag.CutlistClass == "ME")
                    {
                        <li>
                            <iframe id="iframeConsumptionReport" width="100%" height="500" src="~/CutList.aspx?schl=@ViewBag.SCHL.ToString()&class=2&type=REG&Status=@ViewBag.Status" style=" border:none; padding:0; margin:0;"></iframe>
                        </li>
                    }
                    @if (ViewBag.CutlistClass == "MO" || ViewBag.CutlistClass == "MOE")
                    {
                        <li>
                            <iframe id="iframeConsumptionReport" width="100%" height="500" src="~/CutList.aspx?schl=@ViewBag.SCHL.ToString()&class=2&Type=OPEN&Status=@ViewBag.Status" style=" border:none; padding:0; margin:0;"></iframe>
                        </li>
                    }
                    @if (ViewBag.CutlistClass == "S" || ViewBag.CutlistClass == "SE")
                    {
                        <li>
                            <iframe id="iframeConsumptionReport" width="100%" height="500" src="~/CutList.aspx?schl=@ViewBag.SCHL.ToString()&class=4&Type=REG&Status=@ViewBag.Status" style=" border:none; padding:0; margin:0;"></iframe>
                        </li>
                    }
                    @if (ViewBag.CutlistClass == "SO" || ViewBag.CutlistClass == "SOE")
                    {
                        <li>
                            <iframe id="iframeConsumptionReport" width="100%" height="500" src="~/CutList.aspx?schl=@ViewBag.SCHL.ToString()&class=4&Type=OPEN&Status=@ViewBag.Status" style=" border:none; padding:0; margin:0;"></iframe>
                        </li>
                    }

                </ul>
            </div>
        </div>


        <script>
            jQuery(window).load(function () {
                jQuery('#loading-image').hide();
            });
        </script>






