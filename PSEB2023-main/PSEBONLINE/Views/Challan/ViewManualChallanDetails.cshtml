<script type="text/javascript">

    $(function () {
        $(".date").datepicker({
            dateFormat: "dd/mm/yy",
            changeMonth: true,
            changeYear: true,
            yearRange: '2019:' + (new Date().getFullYear())

        });
    });




    function VerifyManualPSEBHOD(k, challanid, challandateV, TotFee) {
        var flag = 0;
        var appstatus = $("#AppStatus-" + challanid).val();



        if (appstatus == "" || appstatus == "0") {
            $("#AppStatus-" + challanid).addClass("input-validation-error");
            flag = 1;
        }

        if (TotFee > 0) {
            if ($("#depositdate-" + challanid).val() == "") {
                $("#depositdate-" + challanid).addClass("input-validation-error");
                flag = 1;
            }
            if ($("#receiptnumber-" + challanid).val() == "") {
                $("#receiptnumber-" + challanid).addClass("input-validation-error");
                flag = 1;
            }

        }
        if (appstatus == "R" || appstatus == "C") {
            if ($("#depositremarks-" + challanid).val() == "") {
                $("#depositremarks-" + challanid).addClass("input-validation-error");
                flag = 1;
            }
        }

        if ($("#seldist-" + challanid).val() == "" || $("#seldist-" + challanid).val() == "0") {
            $("#seldist-" + challanid).addClass("input-validation-error");
            flag = 1;
        }

        if (flag == 1) {
            return false;
        }
        else {

            $('#loading1').html("<p><img src='https://registration2022.pseb.ac.in/images/loadingk.gif'></p>");
            $.ajax({
                type: "POST",
                url: "../Challan/VerifyManualPSEBHOD", // this for calling the web method function in cs code.
                data: '{feedistrict: "' + $("#seldist-" + challanid).val() + '",approvalstatus: "' + appstatus + '",receiptnumber: "' + $("#receiptnumber-" + challanid).val() + '",depositdate: "' + $("#depositdate-" + challanid).val() + '",challanid: "' + challanid + '",challandateV:"' + challandateV.substring(0, 10) + '",brcode: "203",depositremarks: "' + $("#depositremarks-" + challanid).val() + '",OldReceiptNo: "' + $("#OldReceiptNo-" + challanid).val() + '", OldDepositDate: "' + $("#OldDepositDate-" + challanid).val() + '", OldChallanId: "' + $("#OldChallanId-" + challanid).val() + '", OldAmount: "' + $("#OldAmount-" + challanid).val() + '"}',
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (response) {
                    if (response.sn == "Yes") {
                        alert("Challan Verified Successfully");
                        $("#myModal1-" + challanid).modal('hide');
                        $('#loading1').empty();
                    }
                    else if (response.sn == "date") {
                        $('#loading1').empty();
                        alert("Please Select DEPOSIT DATE Must be Greater than CHALLAN DATE and less than equal to VALID DATE");
                    }
                    else {
                        $('#loading1').empty();
                        alert("Challan Not Verified.. Please Try Again");
                    }
                },
                failure: function (response) {
                    alert(response);
                }
            });
        }
    }




    function ReceiptUpdateManual(k, challanid, feecode, appno, Schl, challancategory) {

        var flag = 0;




        var files = $("#ReceiptUpdate-" + challanid).get(0).files;
        var fileData = new FormData();
        for (var i = 0; i < files.length; i++) {
            fileData.append("ReceiptUpdate", files[i]);
        }

        if ($("#ReceiptUpdate-" + challanid).get(0).files.length === 0) {
            $("#ReceiptUpdate-" + challanid).addClass("input-validation-error");
            flag = 1;
        }

        //fileData.append("ReceiptUpdate", ReceiptUpdate);
        fileData.append("challanid", challanid);
        fileData.append("appno", appno);
        fileData.append("feecode", feecode);
        fileData.append("Schl", Schl);
        fileData.append("challancategory", challancategory);
        if (flag == 1) {
            return false;
        }
        else {

            $("#loading1" + k).html("<p><img src='https://registration2022.pseb.ac.in/images/loadingk.gif'></p>");
            $.ajax({
                url: '/Master/JqReceiptUpdateManual',
                type: "POST",
                contentType: false, // Not to set any content header
                processData: false, // Not to process data
                data: fileData,
                success: function (response) {
                    $('#loading1').empty();
                    if (response.oid == 1) {
                        alert("Updated Successfully");
                        $("#myModal1-" + challanid).modal('hide');
                        window.location.href = '/Challan/ViewManualChallanDetails';
                    }
                    else {
                        alert(response.msg);
                    }

                },
                error: function (err) {
                    $('#loading1').empty();
                    alert("Failure");
                    $("#myModal1-" + challanid).modal('hide');
                }
            });
        }
    }


</script>



