@model PSEBONLINE.Models.Import
@{
    Layout = "~/Views/Shared/_Headerui.cshtml";
}
@using (Html.BeginForm("Import12thFail", "ImportData", FormMethod.Post, new { id = "searchform", enctype = "multipart/form-data" }))
{
