@model PSEBONLINE.Models.ReportModel
@{
    ViewBag.Title = "Examination Report";
    Layout = "~/Views/Shared/_Card_Header.cshtml";
}
<style type="text/css">
    td{font-size:11px;padding:1px;vertical-align:top}
</style>
<link rel="stylesheet" href="https://registration2022.pseb.ac.in/Content/Print-4P.css" />
<h1 class="noprint">Reg. No. Status</h1>
<div class="row">
    <div class="col-md-12 text-right noprint">
        <input type="button" value="Print" class="btn" onclick="window.print()" />
        <input type="button" value="Back" class="btn" onclick="history.go(-1);" />
    </div>
    <div class="clearfix"></div>
    <div class="col-md-12">

@if (ViewBag.TotalCount > 0)
{
        <table style="border-collapse:collapse;width:100%;font-family:Arial, Helvetica, sans-serif;font-size:12px" border="1">
            <tr>
                <td style="border-right:0 !important">
                    <div style="padding:2px"><img src="~/Images/Printlogo.jpg" width="60px" height="60px" alt="PSEB" /></div>
                </td>
                <td colspan="5" style="font-size:14px;text-align:center;line-height:22px;vertical-align:top;border-left:0 !important;border-right:0 !important;padding-top:8px;">
                    <span style="font-size:28px">Punjab School Education Board</span>
                    <br />
                    <span>Registration No. Allotment Status of Other Board/State Candidates (@Session["Session"].ToString().Trim())</span>
                </td>
                <td style="border-left:0 !important" align="right">Print Date : @DateTime.Today.ToString("dd/MM/yyyy") &nbsp;</td>
            </tr>
            <tr>               
                <td width="13%" align="center">Class</td>
                <td width="13%" align="center">Form Name
                </td>
                <td width="15%" align="center">Total
                </td>
                <td width="16%" align="center">Already Alloted
                </td>
                <td width="15%" align="center">New Alloted
                </td>
                <td width="16%" align="center">Pending Reg No.
                </td>
                <td width="13%" align="center">Error Marked
                </td>               							
                
            </tr>
@foreach (System.Data.DataRow dr in Model.StoreAllData.Tables[0].Rows)
{
            <tr>               
                <td align="center">@dr["class"].ToString()</td>
                <td align="center">@dr["form_Name"].ToString()</td>
                <td align="right">@dr["tot"].ToString()&nbsp;</td>
                <td align="right">@dr["allready"].ToString()&nbsp;</td>
                <td align="right">@dr["done"].ToString()&nbsp;</td>
                <td align="right">@dr["pend"].ToString()&nbsp;</td>
                <td align="right">@dr["err"].ToString()&nbsp;</td>    
                @*<td>@dr["date"].ToString()</td>*@ 
            </tr> 
}          
        </table>
}
        <p style="height:160px"></p>
      
    </div>
</div>

