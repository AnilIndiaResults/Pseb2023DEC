@model PSEBONLINE.Models.ReportModel
@{
    ViewBag.Title = "OnDemand Certificates Summary Report";
    Layout = "~/Views/Shared/_Card_Header.cshtml";
}
<link rel="stylesheet" href="/Content/Print-4P.css" />

<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>
<div class="row">
    <div class="col-md-12 noprint">
        @using (Html.BeginForm("OnDemandCertificatesSummaryReport", "Report", FormMethod.Post, new { id = "uploadform", enctype = "multipart/form-data" }))
        {
            <div class="row">
                <div class="col-md-3">
                    Select Type @Html.DropDownList("SelList", new SelectList(ViewBag.MySch, "Value", "Text", ViewBag.SelectedItem), "--Select---", new { @class = "", id = "SelectItem1", required = "required" })
                </div>
                <div class="col-md-3">
                    Select Class @Html.DropDownList("SelClass", new SelectList(ViewBag.Mycls, "Value", "Text", ViewBag.Selectedcls), "--All---", new { @class = ""})
                </div>
                <div class="col-md-6">
                    <br />
                    @*<input type="submit" name="submit" value="Submit" />*@
                    <input type="submit" name="submit" value="Export To Excel" />
                    <input type="button" value="Back" class="btn" onclick="history.go(-1)" />
                </div>
            </div>
        }
    </div>
</div>

