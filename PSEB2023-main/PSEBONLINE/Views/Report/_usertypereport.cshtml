@model PSEBONLINE.Models.ReportModel
<h1 style="text-align:center">
    Punjab School Education Board<br />
<span style="font-size:16px;color:#333">School Category wise count of Schools for Session  @Session["Session"].ToString()</span>
</h1>
<table class="table table-bordered" id="tsummary">
    <tr class="active-tr">
        <th>School Category</th>
        <th>Abbreviation</th>
        <th>5th Level</th>
        <th>8th Level</th>
        <th>10th Level</th>
        <th>12th Level</th>
        <th>Total Schools</th>
    </tr>
    @{
    if (ViewBag.TotalCount > 0)
    {
    for (int i = 0; i < Model.StoreAllData.Tables[0].Rows.Count; i++)
    {
    var SchoolCategory = Model.StoreAllData.Tables[0].Rows[i]["usertype"].ToString();
    var abbr = Model.StoreAllData.Tables[0].Rows[i]["abbr"].ToString();
    var TotalPri = Model.StoreAllData.Tables[0].Rows[i]["TotalPri"].ToString();
    var TotalMid = Model.StoreAllData.Tables[0].Rows[i]["TotalMid"].ToString();
    var Totalmat = Model.StoreAllData.Tables[0].Rows[i]["Totalmat"].ToString();
    var TotalSrSec = Model.StoreAllData.Tables[0].Rows[i]["TotalSrSec"].ToString();
    var TotalFinalEntries = Model.StoreAllData.Tables[0].Rows[i]["Total"].ToString();
    int Count = i + 1;
    <tr>
        <td>@SchoolCategory</td>
        <td>@abbr</td>
        <td>@TotalPri</td>
        <td>@TotalMid</td>
        <td>@Totalmat</td>
        <td>@TotalSrSec</td>
        <td>@TotalFinalEntries</td>

    </tr>
    }
    }
    else
    {
    <tr>
        <td colspan="8">
            @ViewBag.Message
        </td>
    </tr>
    }
    }
</table>
