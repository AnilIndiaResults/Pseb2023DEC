@{
    ViewBag.Title = "Private Exam Forms Status";
    Layout = "~/Views/Shared/_Card_Header.cshtml";
}
<link rel="stylesheet" href="https://registration2022.pseb.ac.in/Content/cardprintc.css" />
<h1 class="noprint">Private Exam Forms Status</h1>
<div class="row">
    <div class="col-md-12 noprint" style="display:none">
        <fieldset style="margin-bottom:15px">
            <legend>Search</legend>
            <div class="col-md-3">
                Fee Code <input type="text" />
            </div>
            <div class="col-md-3">Fee Head <input type="text" /></div>
            <div class="col-md-2"><br /><input type="button" value="Search" class="btn" /></div>
        </fieldset>
    </div>
    <div class="clearfix"></div>
    <div class="col-md-12 text-right noprint">
        <input type="button" value="Print" class="btn" onclick="window.print()" />
        <input type="button" value="Back" class="btn" onclick="history.go(-1);" />
    </div>
    <div class="clearfix"></div>
    <div class="col-md-12">
        <div style="border:1px solid #fff !important;display:block;margin-bottom:30px">
            <table border="1" width="100%" class="tprint">
                <thead>
                    <tr>
                        <td style="border-right:0 !important" colspan="2" width="15%"><div style="padding:2px"><img src="~/Images/Printlogo.jpg" width="60px" height="60px" alt="PSEB" /></div></td>
                        <td colspan="4" style="font-size:16px;text-align:center;line-height:24px;vertical-align:top;border-left:0 !important;border-right:0 !important;padding-top:8px">
                            <span style="font-size:28px">Punjab School Education Board</span><br /><span>
                                Private Exam Forms Status Session (@if (Session["Session"] != null)
                                {@Session["Session"].ToString()}
                                else { Response.Redirect("~/Login"); })
                            </span>
                        </td>
                        <td style="text-align:right;border-left:0 !important;" width="25%">
                            Print Date/Time : @DateTime.Now.ToString("dd/MM/yyyy / hh:mm: ss") &nbsp;
                            @*Print Date/Time : @DateTime.Now.ToString("dd/MM/yyyy / hh:mm:ss ") &nbsp;*@
                            @*Time : @DateTime.Now.ToString("hh:mm:ss") &nbsp;*@
                        </td>
                    </tr>
                </thead>

                <div class="table-responsive">
                    <table id="t01" class="table table-bordered">
                        <tr class="active-tr">

                            <th width="5%">&nbsp;Srno</th>
                            <th width="5%">&nbsp;Fee Code</th>
                            <th width="29%">&nbsp;Fee Head/Category Name</th>
                            <th width="27%">&nbsp;Registred Candidate</th>
                            <th width="17%">&nbsp;Challan Generated</th>
                            <th width="17%">&nbsp;Fee Verified/Final</th>
                            

                        </tr>
                        @{
                            if (ViewBag.TotalCount > 0)
                            {
                                int d = 0;
                                int Count = 0;

                                for (int i = 0; i < Model.StoreAllData.Tables[0].Rows.Count; i++)
                                {
                                    d = d + 1;
                                    var FEECODE = Model.StoreAllData.Tables[0].Rows[i]["FEECODE"].ToString();
                                    var FEECAT = Model.StoreAllData.Tables[0].Rows[i]["FEECAT"].ToString();
                                    var totReg = Model.StoreAllData.Tables[0].Rows[i]["totReg"].ToString();
                                    var totChlnGen = Model.StoreAllData.Tables[0].Rows[i]["totChlnGen"].ToString();
                                    var totChlnVfd = Model.StoreAllData.Tables[0].Rows[i]["totChlnVfd"].ToString();
                                    Count = Count + 1;


                        <tr>
                            <td>@Count</td>
                            <td>&nbsp;@FEECODE</td>
                            <td>&nbsp;@FEECAT</td>
                            <td>&nbsp;@totReg</td>
                            <td>&nbsp;@totChlnGen</td>
                            <td>&nbsp;@totChlnVfd</td>
                        </tr>

                                }
                                //var totreg3132 = Model.StoreAllData.Tables[1].Rows[0]["FEECODE"].ToString();
                                //var totchlngen3132 = Model.StoreAllData.Tables[1].Rows[0]["FEECAT"].ToString();
                                // var totchlnv3132 = Model.StoreAllData.Tables[1].Rows[0]["totReg"].ToString();
                                var totReg2 = Model.StoreAllData.Tables[1].Rows[0]["totReg"].ToString();
                                var totChlnGen2 = Model.StoreAllData.Tables[1].Rows[0]["totChlnGen"].ToString();
                                var totChlnVfd2 = Model.StoreAllData.Tables[1].Rows[0]["totChlnVfd"].ToString();
                        <tr>
                            <td colspan="3" style="text-align:right;font-size:16px">Total&nbsp;</td>
                            <td style="font-size:16px">&nbsp;@totReg2 </td>
                            <td style="font-size:16px">&nbsp;@totChlnGen2</td>
                            <td style="font-size:16px">&nbsp;@totChlnVfd2</td>
                        </tr>
                            }
                            else
                            {
                        <tr>
                            <td colspan="10">
                                <span class="rqf">@ViewBag.Message</span>
                            </td>
                        </tr>
                            }
                        }


                    </table>

                </div>

            </table>

        </div>

    </div>
