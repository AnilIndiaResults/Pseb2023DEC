@{
    Layout = "~/Views/Shared/_Headerui.cshtml";
}


<div class="container KDBG">
    <style type="text/css">
        #ac-wrapper {
            background: rgba(0, 0, 0, 0.5) none repeat scroll 0 0;
            height: 100%;
            left: 0;
            position: fixed;
            top: 0;
            width: 100%;
            z-index: 1001;
        }

        #popup {
            min-height: 240px;
            left: 0;
            padding: 0;
            position: absolute;
            top: 128px;
            width: 100%;
        }

            #popup table {
                background: #fff none repeat scroll 0 0;
                border: 5px solid #f1f1f1;
                border-radius: 0;
                box-shadow: 0 0 3px 3px #64686e;
                max-width: 500px;
                min-width: 320px;
            }

            #popup .button {
                padding: 10px;
                background: #efba09; /* Old browsers */
                background: -moz-linear-gradient(top, #efba09 0%, #fb9709 50%, #efba09 100%); /* FF3.6-15 */
                background: -webkit-linear-gradient(top, #efba09 0%,#fb9709 50%,#efba09 100%); /* Chrome10-25,Safari5.1-6 */
                background: linear-gradient(to bottom, #efba09 0%,#fb9709 50%,#efba09 100%); /* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
                filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#efba09', endColorstr='#efba09',GradientType=0 ); /* IE6-9 */
            }
    </style>

    <style type="text/css">
        .ullist li {
            padding: 6px 4px;
            font-size: 16px;
        }
            .uist li a {font-weight:normal !important;font-size:20px;line-height:18px
            }
             .uist li  {font-weight:normal !important;font-size:20px;line-height:30px
            }
        .ullist {
            padding-bottom: 10px;
        }
    </style>
    <h1>INA (Internal Assessment) Portal for <b>@Session["Session"].ToString()</b></h1>
    <div class="row">
        <div class="col-md-12">
            <marquee onmouseout="this.setAttribute('scrollamount', 6, 0);" onmouseover="this.setAttribute('scrollamount', 0, 0);" direction="left" scrollamount="6" style="padding:10px 0;cursor:pointer;background-color:#f0ad4e">
                @*Date Extended For Elective Subject Theory UPTO 11/04/2018&nbsp;&nbsp;|&nbsp;&nbsp;!!&nbsp;&nbsp;&nbsp;&nbsp;*@
                Only online submission of INA marks is to be done by schools (Matric/SrSec).Hard copy submission of marks of INA at Punjab School Education Board sub office is not required.Only a copy for record shall be maintained by school.&nbsp;&nbsp;|&nbsp;&nbsp;!!&nbsp;&nbsp;&nbsp;&nbsp;
            </marquee>

            <ul class="ullist">

                @if (ViewBag.Senior == "1")
                {
                    <li><a href="/School/Agree/S"><samp>&#10093;</samp> <samp>09-11-2022</samp> INA Panel For Sr. Secondary (Session <b>@Session["Session"].ToString()</b>)</a></li>
                }
                @if (ViewBag.Matric == "1")
                {
                    <li><a href="/School/Agree/M"><samp>&#10093;</samp> <samp>09-11-2022</samp> INA Panel For Matric (Session <b>@Session["Session"].ToString()</b>)</a></li>                    
                }               
            </ul>
            @*<h1>Instructions</h1>
        <ul class="uist">
         @if (ViewBag.Matric == "1")
        {
         <li><a href="/PDF/InstructionForMatricElectiveSubject.pdf" style="color:#cc0000" target="_blank"><samp>&#10093;</samp> Instruction for Matriculation Elective Subject Theory Online Marks Entry.</a></li>
         }
         <li><a href="/PDF/10th_CCE.pdf" style="color:#cc0000" target="_blank"><samp>&#10093;</samp> Matric Instructions Regarding Filling of CCE & Submission of Hard Copy With Date Schedule.</a></li>
         <li><a href="/PDF/12th_CCE.pdf" style="color:#cc0000;" target="_blank"><samp>&#10093;</samp> Sr. Secondary Instructions Regarding Filling of CCE & Submission of Hard Copy With Date Schedule.</a></li>
         <li><a href="https://files-cdn.pseb.ac.in/pseb_files/schemeofstudies-2017-21-05-844.pdf" style="color:#cc0000;" target="_blank"><samp>&#10093;</samp> Scheme of Studies for 9th And 10th Class for 2017-18 (Amended CCE Scheme)</a></li>
         <li><a href="https://files-cdn.pseb.ac.in/pseb_files/CCE_Scheme_to_Sen._Sec.0001.pdf" style="color:#cc0000;" target="_blank"><samp>&#10093;</samp> Scheme of Studies for 11th And 12th Class for 2017-18 (Amended CCE Scheme)</a></li>
        </ul>*@
        </div>
    </div>


</div>


