                                            </span>
                                        }
                                        else if (Model.schoolAllowForMarksEntry.Panel.ToLower() == "SchoolBasedExams" && Model.schoolAllowForMarksEntry.Cls == "12" && Model.schoolAllowForMarksEntry.IsAllow == "1" && DateTime.Now.Date <= Convert.ToDateTime(Model.schoolAllowForMarksEntry.LastDate).Date)
