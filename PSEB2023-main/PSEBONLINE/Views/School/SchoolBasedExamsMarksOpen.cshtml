<script type="text/javascript">
    $(function () {
        $("#dialog").dialog({
            modal: true,
            autoOpen: false,
            title: "Are you sure to  Final Submit??",
            width: 450,
            height: 250,
            buttons: {
                "Proceed": function () {
                    window.location.href = '@Url.Action("SchoolBasedExamsFinalReportOpen", "School", new {id = @ViewBag.cid })';
                },
                Cancel: function () {
                    $(this).dialog("close");
                }
            }
        });
        $("#btnShow").click(function () {
            $('#dialog').dialog('open');
        });
    });
</script>


@using (Html.BeginForm("SchoolBasedExamsMarksOpen", "School", FormMethod.Post, new { id = "CCE", enctype = "multipart/form-data" }))
{
    DateTime todayDate = DateTime.Now;
