<script type="text/javascript">
    function SubMarks(k, nxt, prv) {
        var tblname = '#subjects-' + k;
        var tblnxt = '#subjects-' + nxt;
        //  alert(tblname);
        //   alert(tblnxt);
        var TableData;
        TableData = JSON.stringify(storeTblValues());
        // console.log("JsonString  = " + TableData + "\n");
        // alert(TableData);
        //post
        if (TableData == "false") {
            alert("Please Fill Subject OBT-Marks, OBTMarks can be in-between Min and Max Marks");
        }
        else if (TableData != "false") {
            // alert('Now Start:' + TableData);
            $('#loading1-' + k).html("<p><img src='https://registration2022.pseb.ac.in/images/loadingk.gif'></p>");
            $.post("/School/JqSchoolBasedExamsMarksOpen", { "stdid": k, "CandSubject": TableData, "cls": '@ViewBag.cid' },
                function (data) {
                    $('#loading1-' + k).empty();
                    //alert('data.status  = ' + data.status);
                    if (data.status == "0" || data.status == "-1") {
                        alert("Subject Marks Alloted Failure, Please Try Again");
                    }
                    else if (data.status == "-2") {
                        alert("Invalid Marks, Please Enter OBT Marks less than equal to Max Marks and Greater than Zero !!");
                    }
                    else if (data.status == "-3") {
                        alert("Please Fill All Subject OBTMARKS");
                    }
                    else {
                        alert("Subject Marks Alloted Successfully of Student : " + k);
                        nxtDiv(nxt, k, prv);
                    }
                });
        }

        function storeTblValues() {
            // alert('3');
            var TableData = new Array();
            $('' + tblname + ' tr').each(function (row, tr) {
                TableData[row] = {
                    //"SUB": $(tr).find('td:eq(0)').text()
                    "SUB": $(tr).find('.chksub').val(),
                    "OBTTEST1": $(tr).find('.chkclass1').val(),
                    "MAXTEST1": $(tr).find('.chkmax1').val(),
                    "OBTTEST2": $(tr).find('.chkclass2').val(),
                    "MAXTEST2": $(tr).find('.chkmax2').val(),
                    "OBTTEST3": $(tr).find('.chkclass3').val(),
                    "MAXTEST3": $(tr).find('.chkmax3').val(),
                    "OBTTEST4": $(tr).find('.chkclass4').val(),
                    "MAXTEST4": $(tr).find('.chkmax4').val(),
                    "OBTTEST5": $(tr).find('.chkclass5').val(),
                    "MAXTEST5": $(tr).find('.chkmax5').val(),
                }
            });
            TableData.shift();  // first row will be empty - so remove
            //alert(TableData);
            return TableData;

        }
    }

    function nxtDiv(nxt, crnt, prv) {
        // var cid = '#myModal1-' + crnt
        $('#myModal1-' + crnt).modal('hide');
        if (nxt == "0" && prv == "0") { }
        else if (nxt == "0" && prv != "0") {
            $('#myModal1-' + prv).modal('show');
        }
        else if (prv == "0" && nxt != "0") {
            $('#myModal1-' + nxt).modal('show');
        }
        else if (prv != "0" && nxt != "0") {
            $('#myModal1-' + nxt).modal('show');
        }
    }


</script>
<script type="text/javascript">

    function pad(obj, length) {
        // alert("pad:- " + obj)
        var text = obj.value;
        // alert("pad v:- " + text)
        var str = '' + text;
        // alert("pad str:- " + str)
        while (str.length < length) {
            str = '0' + str;
        }
        if (obj.value == 'A' || obj.value == 'C' || obj.value == 'a' || obj.value == 'c') { }
        else {
            obj.value = str;
        }
    }

    function CheckMarks(obj, k, marks) {

        if (obj.value == 'A' || obj.value == 'C' || obj.value == 'a' || obj.value == 'c' || obj.value == '') {
            if (obj.value == 'A' || obj.value == 'a') {
                obj.value = 'ABS';
            }
            else if (obj.value == 'C' || obj.value == 'c') {
                obj.value = 'CAN';
            }
            return true;
        }
        else if (obj.value > marks || obj.value < 0) {
            obj.value = '';
            alert("Invalid Marks, Please Enter OBT Marks less than equal to Max Marks and Greater than Zero !!");
            return false;
        }
        else {
            // alert("call pad");
            pad(obj, 3);
            return true;
        }
    }

</script>

<script>
    $(document).ready(function () {
        $(".chkclass").off('keypress');

        $(".chkclass").keypress(function (evt) {
            var charCode = (evt.which) ? evt.which : evt.keyCode
            if ((charCode >= 48 && charCode <= 57) || (charCode == 9) || (charCode == 8) || (charCode == 65) || (charCode == 67) || (charCode == 97) || (charCode == 99)) {
                return true;
            }
            else {
                alert("Invalid character, Enter Numeric Value or 'A' or 'C' !!");
                return false;
            }
        });


        var value = document.getElementById('SelFilter').value;
        if (value == 6) {
            $("#SearchString").datepicker("enable");
            $("#SearchString").off('keypress');
            $("#SearchString").keypress(function (evt) {
                return false;

            });
            $("#SearchString").datepicker({
                dateFormat: "dd/mm/yy",
                changeMonth: true,
                changeYear: true,
