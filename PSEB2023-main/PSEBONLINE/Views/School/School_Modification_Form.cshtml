@model PSEBONLINE.Models.SchoolModels
@{
    Layout = "~/Views/Shared/_Header.cshtml";
}
<h1>School Modification Form</h1>

<h4>School Identity Details</h4>

@using (Html.BeginForm("School_Modification_Form", "School", FormMethod.Post, new { id = "searchform", enctype = "multipart/form-data" }))
{
<div class="row">
    <div class="col-md-4">
        School Code @Html.TextBoxFor(a => a.SCHL, new { id = "SchoolCode", @maxlength = "7", @readonly = "readonly" })
    </div>
    <div class="col-md-4">
        ID No. @Html.RadioButtonFor(m => m.SameAsSchl, "Yes", new { id = "Yes" })   <span>Same As Schl</span>
        <br />@Html.TextBoxFor(a => a.idno, new { id = "IDNo", @maxlength = "10"})
    </div>
    <div class="col-md-4">
        Study Center Code @Html.TextBoxFor(a => a.OCODE, new { id = "OCODE", @maxlength = "7", onkeypress = "return isNumberKey(event);" })
    </div>
    <div class="clearfix"></div>
    <div class="col-md-4">
        Session
        @Html.DropDownList("Session", new SelectList(ViewBag.SessionList, "Value", "Text"), "--Select Session---", new { id = "Session", required = "Required" }) @*required = "Required"*@

    </div>
    <div class="col-md-4">
        School Type
        @Html.DropDownList("USERTYPE", new SelectList(ViewBag.SchoolTypeList, "Value", "Text"), "--Select School Type---", new { id = "USERTYPE" })
    </div>
    <div class="col-md-4">
        Class
        @Html.DropDownList("CLASS", new SelectList(ViewBag.ClassTypeList, "Value", "Text"), "--Select Class---", new { id = "CLASS" })

    </div>
    <div class="clearfix"></div>

    <div class="col-md-4">
        Status
        @Html.DropDownList("Status", new SelectList(ViewBag.StatusList, "Value", "Text"), new { id = "Status", @readonly = "readonly" })
    </div>
    <div class="col-md-4">
        Approved
        @Html.DropDownList("Approved", new SelectList(ViewBag.YesNoList, "Value", "Text"), new { id = "Approved" })
    </div>
    <div class="col-md-4" style="display:none">
        Verified  @Html.DropDownList("vflag", new SelectList(ViewBag.YesNoList, "Value", "Text"), new { id = "vflag" })
    </div>

    <div class="col-md-4" style="display:none">
        Password @Html.TextBoxFor(a => a.PASSWORD, new { @class = "textnone", id = "PASSWORD", @maxlength = "20" })

    </div>

    <div class="col-md-4">
        Validity @Html.TextBoxFor(a => a.VALIDITY, new { @Value = "9999", id = "VALIDITY", @maxlength = "10", onkeypress = "return isNumberKey(event);" })
    </div>
    <div class="clearfix"></div>

