@model PSEBONLINE.Models.OnlineCentreCreationsChallanViews
@{
    ViewBag.Title = "Print Letter";
    Layout = "~/Views/Shared/_Card_Header.cshtml";
}
@*<link rel="stylesheet" href="/Content/Print-4P-SVF.css" />*@
<link rel="stylesheet" href="https://registration2022.pseb.ac.in/Content/Print-4L-SRR.css" />   
<style type="text/css">
    .kds-td {
        border-collapse: collapse
    }

        .kds-td td {
            padding: 5px;
        }
</style>
<h1 class="noprint">Online Centre Creation Print</h1>
<table style="width:100%;" border="0" class="noprint">
    <tr>
        <td style="text-align:right;padding-bottom:3px">
            <input type="button" value="Print" class="btn" onclick="window.print()" />
            @*<input type="button" value="Back" class="btn" onclick="history.go(-1);" />*@
        </td>
    </tr>
</table>
<div class="table-responsive">
    <table width="100%" border="0" class="kds-td">
        <tr>
            <td style="font-size:14px;text-align:center;line-height:20px;vertical-align:top;border-bottom:2px solid #000 !important">
                <span style="font-size:28px">PUNJAB SCHOOL EDUCATION BOARD</span>
                <br />
                <span>
                    Online Centre Creation Print
                </span>
            </td>
        </tr>
    </table>
    <table width="100%" border="1" class="kds-td">
        <tr>
            <td width="19%">School Code</td>
            <td width="15%">@Html.DisplayFor(a => a.SCHL, new { })</td>
            <td width="19%">USER TYPE</td>
            <td width="15%">@Html.DisplayFor(a => a.USERTYPE, new { })</td>
            <td width="19%"></td>
            <td width="13%"></td>
        </tr>
        <tr>

            <td>
                School Name (English)
            </td>
            <td colspan="5">@Html.DisplayFor(a => a.SCHLNME, new { }) </td>
        </tr>
        <tr>
            <td>
                School Name (Punjabi)
            </td>

            <td colspan="5"><span class="kdsfont">@Html.DisplayFor(a => a.SCHLNMP, new { }) </span></td>
        </tr>

        <tr>
            <td>
                DISTNM  (English)
            </td>
            <td>@Html.DisplayFor(a => a.DISTNM, new { }) </td>
            <td>
                DISTNM (Punjabi) <span class="rqf">*</span>
            </td>
            <td><span class="kdsfont">@Html.DisplayFor(a => a.DISTNMP, new { })  </span></td>
            <td colspan="2"></td>
        </tr>
        <tr>

            <td>
                Self School Matric Students
            </td>
            <td>@Html.DisplayFor(a => a.MATREG, new { }) </td>
            <td>
                Self School Senior Students
            </td>
            <td>@Html.DisplayFor(a => a.SSREG, new { }) </td>
            <td>
                Self School Middle Students
            </td>
            <td>@Html.DisplayFor(a => a.MIDREG, new { }) </td>
        </tr>

        <tr>
            <td colspan="6" style="background-color:#c4c4c4;font-weight:bold">Centre Details</td>
        </tr>
        <tr>
            <td>
                Total Capacity of Exam Centre
            </td>
            <td>
                @Html.DisplayFor(a => a.TotalCapacity, new { })
            </td>

            <td>
                Total No. of Room  For Exam Centre
            </td>
            <td>
                @Html.DisplayFor(a => a.TotalRooms, new { })
            </td>
            <td>Total No. of Room Other Than Exam Centre </td>
            <td>
                @Html.DisplayFor(a => a.TotalRoomsOther, new { })
            </td>
        </tr>
        <tr>
            <td>
                Single Bench Furniture <span class="rqf">*</span>
            </td>
            <td>
                @Html.DisplayFor(a => a.SingleBenchFurniture, new { })
            </td>
            <td>
                Double Bench Furniture <span class="rqf">*</span>
            </td>
            <td>
                @Html.DisplayFor(a => a.DoubleBenchFurniture, new { })
            </td>
            <td colspan="2"></td>
        </tr>
        <tr>
            <td colspan="6" style="background-color:#c4c4c4;font-weight:bold">Payment Detail</td>
        </tr>
        <tr>
            <td colspan="6" style="padding:0">
                @if (Model.ChallanVerify == 1)
                {

                    <table width="100%" border="1" class="kds-td">
                        <tr style="background-color:#c4c4c4;font-weight:bold">
                            <th>Ref. No.</th>
                            <th>Bank</th>
                            <th>Payment ID</th>
                            <th>Trans On</th>
                            <th>Amount</th>
                            <th>Trans Id</th>
                            <th>Deposit On</th>
                        </tr>
                        <tr>
                            <td>@Model.CentreAppNo </td>
                            <td>@Model.BANK </td>
                            <td>  @Model.Challanid</td>
                            <td>@Model.Challandt</td>
                            <td>@Model.TotalFee</td>
                            <td>  @Model.J_REF_NO</td>
                            <td>@Model.DEPOSITDT</td>
                        </tr>
                    </table>
                }
            </td>
        </tr>
    </table>
</div>

