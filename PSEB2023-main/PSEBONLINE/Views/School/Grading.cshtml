        // console.log("JsonString  = " + TableData + "\n");
        // alert(TableData);
        //post
        if (TableData == "false") {
            alert("Please Fill Theory Marks's as per Instructions");
        }
        else if (TableData != "false") {
            // alert('Now Start:' + TableData);
            $('#loading1-' + k).html("<p><img src='https://registration2022.pseb.ac.in/images/loadingk.gif'></p>");
            $.post("/School/JqGrading", { "stdid": k, "CandSubject": TableData },
                function (data) {
                    //console.log('1');
                    //   console.log('data' + data.status);
                    $('#loading1-' + k).empty();
                    //  alert('data.status  = ' + data.status);
                    if (data.status == "0" || data.status == "-1") {
                        alert("Theory Marks Alloted Failure, Please Try Again");
                    }
                    else if (data.status == "-2") {
                        alert("Invalid Marks, Please Enter Theory Marks's as per Instructions !!");
                    }
                    else {
                        alert("Theory Marks Alloted Successfully of Student : " + k);
                        nxtDiv(nxt, k, prv);
                    }
                });
        }

        function storeTblValues() {
            // alert('3');
            var TableData = new Array();
            $('' + tblname + ' tr').each(function (row, tr) {
                TableData[row] = {
                    //"SUB": $(tr).find('td:eq(0)').text()
                    "SUB": $(tr).find('.chksub').val()
                    , "OBTMARKS": $(tr).find('.chkclass').val()
                    , "MINMARKS": $(tr).find('.chkmin').val()
                    , "MAXMARKS": $(tr).find('.chkmax').val()
                }
            });
            TableData.shift();  // first row will be empty - so remove
            //alert(TableData);
            return TableData;

        }
    }

    function nxtDiv(nxt, crnt, prv) {
        // var cid = '#myModal1-' + crnt
        $('#myModal1-' + crnt).modal('hide');
        if (nxt == "0" && prv == "0") { }
        else if (nxt == "0" && prv != "0") {
            $('#myModal1-' + prv).modal('show');
        }
        else if (prv == "0" && nxt != "0") {
            $('#myModal1-' + nxt).modal('show');
        }
        else if (prv != "0" && nxt != "0") {
            $('#myModal1-' + nxt).modal('show');
        }
    }


</script>
