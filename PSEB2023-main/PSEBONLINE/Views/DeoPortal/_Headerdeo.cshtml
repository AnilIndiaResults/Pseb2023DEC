<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title Punjab School Education Board</title>
    <link href='https://fonts.googleapis.com/css?family=Source+Sans+Pro:400,400italic,600' rel='stylesheet' type='text/css'>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>
    <link rel="stylesheet" href="http://code.jquery.com/ui/1.12.0/themes/base/jquery-ui.css">   
 <script src="~/Scripts/jquery-1.7.1.min.js"></script>
    <script src="~/Scripts/jquery.validate.min.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>
           <noscript>
            <style type="text/css">
                #HPL {display:none}
            </style>
        <charset style="background-color:#cc0000;color:#ffd800;font-size:24px;text-align:center;padding:10px 0">Your browser does not support JavaScript!</charset>
</noscript>   
     @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <div id="HPL">
     <div class="header">
        <div class="container">
            <div class="row">
                <div class="col-xs-3 col-md-3"><img src="~/Images/pseb_logo.png" alt="Punjab School Education Board" class="img-responsive" /></div>
                <div class="col-xs-9 col-md-6"><center> <img src="~/Images/PSEB_Text.png" alt="Punjab School Education Board" class="img-responsive" /></center></div>
                <div class="col-xs-12 col-md-3 text-right"></div>
                <div class="clearfix"></div>
                <div class="col-md-12 text-center" style="color:#fff;font-size:24px;">District Education Officer Portal (Session 2016-17)</div>
                <div class="col-md-12 text-center" style="color:#fff">
                    Welcome: @if (Session["NAME"] != null)
            {@Session["NAME"].ToString()}
            else { Response.Redirect("/DeoPortal"); }
                </div>
            </div></div></div>
       
    <div class="navbar navbar-inverse">
        <div class="container">
            <div class="row">
                <div class="navbar-header">
                    <button type = "button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>                   
                </div>
                <div class="navbar-collapse collapse">
                    <ul class="nav navbar-nav">
                        
                        @if (Session["NAME"].ToString() == "admin" || Session["NAME"].ToString() == "ADMIN")
                        {
                            <li>@Html.ActionLink("Home", "DeoHome", "DeoPortal")</li>
                            <li>@Html.ActionLink("Profile", "DeoProfileDetails", "DeoPortal")</li>
                            <li>@Html.ActionLink("Admin View Staff", "AdminViewStaff", "DeoPortal")</li>
							<li class="dropdown">
                            <a class="dropdown-toggle" data-toggle="dropdown">
                                Report
                                <span class="caret"></span>
                            </a>
                            <ul class="dropdown-menu">
							<li>@Html.ActionLink("Summary Report", "DeoSummaryReport", "DeoPortal")</li>
							<li>@Html.ActionLink("Examiner Summary Report", "ExaminerSummaryReport", "DeoPortal")</li>
							</ul> 
                                </li>                           
                            <li>@Html.ActionLink("Staff Letter Print", "StaffLetter", "DeoPortal")</li>
                            <li>@Html.ActionLink("Mail To Deo", "MailToDeo", "DeoPortal")</li>                            
							<li>@Html.ActionLink("Notice Board", "NoticeBoard", "DeoPortal")</li>
                        }
                        else
                        {
                            <li>@Html.ActionLink("Home", "DeoHome", "DeoPortal")</li>
                            <li>@Html.ActionLink("Profile", "DeoProfileDetails", "DeoPortal")</li>
                            <li>@Html.ActionLink("Centre", "CenterList", "DeoPortal")</li>
                            <li>@Html.ActionLink("Cluster", "ClusterList", "DeoPortal")</li>
                            <li>@Html.ActionLink("Final Submit", "FinalSubmitDeoPortal", "DeoPortal")</li>
                            <li>@Html.ActionLink("Final Print", "FinalPrint", "DeoPortal")</li>
                            <li>@Html.ActionLink("Examiner", "Examiner", "DeoPortal")</li>
							<li>@Html.ActionLink("Notice Board", "NoticeBoard", "DeoPortal")</li>
                            <li>@Html.ActionLink("Replacement Staff", "ReplacementStaff", "DeoPortal")</li>
                        }

						<li>@Html.ActionLink("C-55 Letter Print", "SupervisoryStaffList", "DeoPortal")</li>
                       </ul>
                            <ul class="nav navbar-nav navbar-right">
                                
                                <li>
                                    @Html.ActionLink("LOGOUT", "Logout", "DeoPortal")
                                </li>

                                @*<li><a>LOGOUT</a></li>*@
                            </ul>
</div>
                </div>
            </div>
    </div>

    <div class="container KDBG">
            @RenderBody()

        </div>
<footer class="bs-docs-footer">
        <div class="container text-center">
           All Rights Reserved at Punjab School Education Board, 1969 - 2016 
        </div>
    </footer>
    </div>
       
     @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
<script>
  (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
  (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
  m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
  })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

  ga('create', 'UA-82197953-1', 'auto');
  ga('send', 'pageview');

</script>
</body>
</html>