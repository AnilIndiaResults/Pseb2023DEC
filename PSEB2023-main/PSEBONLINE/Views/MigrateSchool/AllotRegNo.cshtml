        fileData.append('formName', FormName);
        fileData.append('schlDist', schlDist);
        fileData.append('StudentUniqueId', StudentUniqueId);

        //alert('Uploading');
        $("#loading2" + id).html("<p><img src='https://registration2022.pseb.ac.in/images/loadingk.gif'></p>");

        $.ajax({
            url: '/Master/JqUpdateOtherBoardDocuments',
            type: "POST",
            contentType: false, // Not to set any content header
            processData: false, // Not to process data
            data: fileData,
            success: function (response) {
                $("#loading2" + id).empty();
                if (response.oid == 1) {
                    alert("Other Board Documents Updated Successfully");
                    $('#myModalDocument-' + id).modal('hide');
                }
                else {
                    alert(response.msg);
                }

            },
