                </div>
            </div>
            <div class="clearefix"></div>
            <div class="row">
                <div class="col-md-4">
                    Select Document Verifying Employee:  @Html.DropDownList("SelEmp", new SelectList(ViewBag.AllEmpList, "Value", "Text", ViewBag.SelectedEmp), "--All Employee---", new { @class = "" })
