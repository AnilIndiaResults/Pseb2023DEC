            <script type="text/javascript">

                function pad(obj, length) {
                    // alert("pad:- " + obj)
                    var text = obj.value;
                    // alert("pad v:- " + text)
                    var str = '' + text;
                    // alert("pad str:- " + str)
                    while (str.length < length) {
                        str = '0' + str;
                    }
                    if (obj.value == '--') { }
                    else {
                        obj.value = str;
                    }
                }


                function CheckMarks(obj, k, l) {
                    var max = 0;
                    //alert('CheckMarks');
                    // alert(obj.value);
                    //Sub_1_Th_Obt
                    // var tblname = '#sid-' + k;
                    var tblname = '#Sub_' + k + '_' + l + '_Obt';
