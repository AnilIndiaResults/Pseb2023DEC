            <script type="text/javascript">

                function pad(obj, length) {
                    // alert("pad:- " + obj)
                    var text = obj.value;
                    // alert("pad v:- " + text)
                    var str = '' + text;
                    // alert("pad str:- " + str)
                    while (str.length < length) {
                        str = '0' + str;
                    }
                    if (obj.value == '--') { }
                    else {
                        obj.value = str;
                    }
                }

                function CheckMarks(obj, k, l) {
                    var max = 0;
                    // alert('CheckMarks');
                    // alert(obj.value);
                    //Sub_1_Th_Obt
                    // var tblname = '#sid-' + k;
                    var tblname = '#Sub_' + k + '_' + l + '_Obt';
                    // alert(tblname);
                    var tblMax = '#Sub_' + k + '_' + l + '_Max';
                    // alert(tblMax);
                    // Sub_1_Th_Max
                    // alert('max' + max);
                    // var Sub_1_Th_Max = $("#Sub_1_Th_Max").val();
                    // alert('Sub_1_Th_Max : ' + Sub_1_Th_Max);

                    // var tblMax = '#Sub_'+k+'_Th_Max';
                    var max1 = $(tblMax).val();
                    var max = parseInt(max1, 10);
                    // alert('Sub_1_Th_Max  : ' + max);// 050
                    // var max = parseInt(max,10);
                    // alert('max int : ' + max);

                    //alert('fobj' + obj.value);
                    //alert('fmax' + max1);

                    if (max1 == '--') {
                        obj.value = '--';
                        return true;
                    }
                    else if (obj.value > max || obj.value < 0) {
                        obj.value = '';
                        alert("Invalid Marks, Please Enter OBT Marks less than equal to Max Marks and Greater than Zero !!");
                        return false;
                    }
                    else {
                        // alert("call pad");
                        pad(obj, 3);
                        return true;
                    }
