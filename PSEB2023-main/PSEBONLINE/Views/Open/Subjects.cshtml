            <script type="text/javascript" language="javascript">
                alert("Please Select Appearing Year");
                 window.location.href = '@Url.Action("Registration", "Open")';
            </script>
        }
        else
        {
            <script type="text/javascript" language="javascript">
                 alert("Subject Saved Failure, Plz Try Again ");
