@model PSEBONLINE.Models.OpenUserRegistration
@{
    ViewBag.Title = "Subjects";
    Layout = "~/Views/Shared/_HeaderOPEN.cshtml";
}

@*<h1>Subjects</h1>*@
<section class="content-header">
    @if (Session["app_no"] != null)
    {
        if (Session["app_class"].ToString() == "12")
        {<h1>Open School Sr. Secondary Registration</h1>}
        else
        {<h1>Open School Matric Registration</h1> }
        <ol class="breadcrumb">
            <li><a href="/open/Applicationstatus/@Session["app_no"].ToString()"><i class="fa fa-home"></i> Home</a></li>
            <li class="active">Subjects</li>
        </ol>
        @*Welcome :-
            @Html.DisplayFor(model => model.NAME, new { htmlAttributes = new { @class = "form-control" } })
            (@Session["app_no"].ToString())*@
    }
</section>
<div class="box">
    <div class="row">
        <div class="col-md-2"> Group / <span class="kdsfont">¬ÏÝóÆ</span></div>
        <div class="col-md-4"><label id="group" style="@(Session["app_class"].ToString() == "12" ? "display:block" : "display:none")">@Session["app_stream"].ToString()</label></div>
        <div class="col-md-2"> Category </div>
        <div class="col-md-4"><label>@Html.DisplayFor(model => model.CAT, new { htmlAttributes = new { @class = "form-control" } })</label></div>
        <div class="clearfix"></div>
        <div class="col-md-2">Board </div>
        <div class="col-md-4"><label>@Html.DisplayFor(model => model.BOARD, new { htmlAttributes = new { @class = "form-control" } })</label></div>
        <div class="col-md-2">Appearing Session Year </div>
        <div class="col-md-4"><label>@Html.DisplayFor(model => model.AppearingYear, new { htmlAttributes = new { @class = "form-control" } })</label></div>
        <div class="col-md-2"> Passing/Previous Session </div>
        <div class="col-md-4"><label>@Html.DisplayFor(model => model.OSESSION, new { htmlAttributes = new { @class = "form-control" } })</label></div>
        <div class="clearfix"></div>
    </div>
</div>

@{
    if (ViewData["Result"] != null)
    {
        if (ViewData["Result"].ToString() == "1")
        {
            <script type="text/javascript" language="javascript">
                alert("Subject Saved Successfully ");
                  window.location.href = '@Url.Action("ViewRegistration", "Open", new { Id = Session["app_no"].ToString() })';
            </script>

        }
        else if (ViewData["Result"].ToString() == "2")
        {
            <script type="text/javascript" language="javascript">
                alert("Duplicate Subjects, Plz Check and Fill Carefully");
                window.location.href = '@Url.Action("Subjects", "Open")';
            </script>

        }
        else if (ViewData["Result"].ToString() == "CC")
        {
            <script type="text/javascript" language="javascript">
                alert("Please fill CREDIT CARRY Marks and verify your Marks before final submission ");
                 window.location.href = '@Url.Action("Subjects", "Open")';
            </script>
        }
        else if (ViewData["Result"].ToString() == "ML3")
        {
            <script type="text/javascript" language="javascript">
                 //alert("you can choose either PUNJABI (ELECTIVE) or HINDI (ELECTIVE) ");
                alert("You cannot select more than 3 language subject");
                 window.location.href = '@Url.Action("Subjects", "Open")';
            </script>
        }

        else if (ViewData["Result"].ToString() == "ML1")
        {
            <script type="text/javascript" language="javascript">
                 //alert("you can choose either PUNJABI (ELECTIVE) or HINDI (ELECTIVE) ");
                alert("You cannot select more than two language subject");
                 window.location.href = '@Url.Action("Subjects", "Open")';
            </script>
        }
        else if (ViewData["Result"].ToString() == "ML2")
        {
            <script type="text/javascript" language="javascript">
                 //alert("you can choose either MUSIC (VOCAL) or MUSIC (INSTRUMENTAL) or MUSIC (TABLA) ");
                alert("You cannot select more than two music subject");
                 window.location.href = '@Url.Action("Subjects", "Open")';
            </script>
        }
        else if (ViewData["Result"].ToString() == "ML3")
        {
            //You an adopt max 3 languages
            <script type="text/javascript" language="javascript">
                alert("You cannot select more than three language");
                 window.location.href = '@Url.Action("Subjects", "Open")';
            </script>
        }
        else if (ViewData["Result"].ToString() == "5")
        {
            <script type="text/javascript" language="javascript">
                alert("Duplicate Subjects");
                 window.location.href = '@Url.Action("Subjects", "Open")';
            </script>
        }
        else if (ViewData["Result"].ToString() == "12NA" || ViewData["Result"].ToString() == "10NA")
        {
            <script type="text/javascript" language="javascript">
                alert("Not Allowed for CC");
                 window.location.href = '@Url.Action("Subjects", "Open")';
            </script>
        }
        else if (ViewData["Result"].ToString() == "AY")
