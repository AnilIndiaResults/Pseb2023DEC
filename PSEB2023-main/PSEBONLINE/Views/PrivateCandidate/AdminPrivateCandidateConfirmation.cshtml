                filerdr.readAsDataURL(input.files[0]);
                $("#errmsg").hide();

            }
            else {
                if (parseInt(size) <= 5) {
                    alert("Your file size is too Small that is " + size + " KB.");
                    $('#Photo').val('');
                    ('#imgPhoto').hide();
                }
                if (parseInt(size) > 50) {
                    alert("Your file size is too large that is " + size + " KB.");
                    $('#std_Photo').val('');
                    ('#imgPhoto').hide();
                }


            }
        }

    }
    function findSizeAndPreviewSign(input) {
        if (typeof ($("#std_Sign")[0].files) != "undefined") {

            var size = parseFloat($("#std_Sign")[0].files[0].size / 1024).toFixed(2);
            // alert(size);
            if (parseInt(size) >= 1 && parseInt(size) <= 50) {
                var filerdrS = new FileReader();
                filerdrS.onload = function (e) {
                    $('#imgSign').attr('src', e.target.result);

                }
                filerdrS.readAsDataURL(input.files[0]);
                $("#errmsg").hide();

            }
            else {
                if (parseInt(size) <= 1) {
                    alert("Your file size is too Small that is " + size + " KB.");
                    $('#std_Sign').val('');
                    ('#imgSign').hide();
                }
                if (parseInt(size) > 50) {
                    alert("Your file size is too large that is " + size + " KB.");
                    $('#Sign').val('');
                    ('#imgSign').hide();
                }

            }
        }

    }

    function val() {
        if (!window.confirm("Are you Sure to Final Submit. Once Final Submitted you will not able to make any kind of changes in Application ?")) {
            return false;
        }
        else {
            document.location.href = '@Url.Action("FinalSubmit", "PrivateCandidate")';
        }
    }

</script>



