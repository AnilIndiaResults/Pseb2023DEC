                @{
                    var Cat1 = Model.StoreAllData.Tables[0].Rows[0]["cat"].ToString();
                    var FormStatus3 = Model.StoreAllData.Tables[0].Rows[0]["FormStatus"].ToString();
                    @Html.HiddenFor(m => m.FormStatus)
                    if (FormStatus3 == "2" || FormStatus3 == "3" || Cat1 == "R" || Cat1 == "D")
                    {
                        <div class="col-md-2">Exam District <span class="rqf"></span></div>
                        <div class="col-md-4">@Html.DropDownList("SelExamDist", new SelectList(ViewBag.MyExamDist, "Value", "Text"), "--Select---", new { @class = "", @disabled = "disabled" })</div>
                        @*<div class="col-md-2">@Html.TextBoxFor(a => a.SelExamDist, new { id = "SelExamDist", @ReadOnly = "ReadOnly" })</div>*@

                    }
                    else
