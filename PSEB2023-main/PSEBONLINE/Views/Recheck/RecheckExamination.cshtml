@{
    if (ViewData["Status"] != null)
    {
        var result = ViewData["Status"].ToString();
        if (result == "1")
        {
            <script type="text/javascript" language="javascript">
                // alert("Refrence No. Successfully Generated. Kindly Check your Email and Mobile No.: ");
                document.location.href = '@Url.Action("RecheckConfirmation/"+ ViewData["refno"], "Recheck")';
            </script>
        }
        if (result == "2")
        {
            <script type="text/javascript" language="javascript">
                //alert("Refrence No. Already Generated. Kindly Check your Email and Mobile No. : ");
                document.location.href = '@Url.Action("RecheckConfirmation/" + ViewData["roll"], "Recheck")';
            </script>
        }
        if (result == "0")
        {
            <script type="text/javascript" language="javascript">
                alert("Refrence No. Not Generated. Retry Again ");
                document.location.href = '@Url.Action("RecheckExamination", "Recheck")';
            </script>
        }
    }
}
<script type="text/javascript">
    function valForm() {
        var txtrefNo = document.getElementById("refNo").value;
        if (txtrefNo.value == "") {
            alert("Enter Reference Number");
            txtrefNo.focus();
            return false;
        }
        if (txtrefNo.length <= 8) {
            alert("Enter Valid Reference Number.");
            document.getElementById("refNo").focus();
            return false;
        }
        var txtOROLL = document.getElementById("ROLL");
        if (txtOROLL.value == "") {
            alert("Enter ROLL No.");
            txtOROLL.focus();
            return false;
        }
    }
    function isNumberKey(evt) {
        var charCode = (evt.which) ? evt.which : event.keyCode
        if ((charCode >= 48 && charCode <= 57) || (charCode == 8) || (charCode == 46)) {
            return true;
        }
        else {
            alert("Invalid character...Enter Only Number!!");
            return false;
        }
    }
</script>
